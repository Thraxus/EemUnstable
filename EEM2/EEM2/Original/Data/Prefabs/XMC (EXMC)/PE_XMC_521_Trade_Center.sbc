// &lt;&lt; AUTO INSTALLATION &gt;&gt;

bool Install () {
    List &lt;IMyTerminalBlock&gt; _blocks = new List &lt;IMyTerminalBlock&gt; ();
    // ASSEMBLERS
    GridTerminalSystem.GetBlocksOfType &lt;IMyAssembler&gt; (_blocks);
    for(int i = 0; i &lt; _blocks.Count; i++) {
        if(!_blocks [i].CustomName.Contains (productionID))
            _blocks [i].SetCustomName (_blocks [i].CustomName + " " + productionID);
    }
    // REFINERIES
    GridTerminalSystem.GetBlocksOfType &lt;IMyRefinery&gt; (_blocks);
    for(int i = 0; i &lt; _blocks.Count; i++) {
        if(!_blocks [i].CustomName.Contains (productionID))
            _blocks [i].SetCustomName (_blocks [i].CustomName + " " + productionID);
    }
    // SORTERS
    GridTerminalSystem.GetBlocksOfType &lt;IMyConveyorSorter&gt; (_blocks);
    for(int i = 0; i &lt; _blocks.Count; i++) {
        if(!_blocks [i].CustomName.Contains (holdID) &amp;&amp; !_blocks [i].CustomName.Contains (outputID))
            _blocks [i].SetCustomName (_blocks [i].CustomName + " " + holdID);
        if(!_blocks [i].CustomName.Contains (sorterID))
            _blocks [i].SetCustomName (_blocks [i].CustomName + " " + sorterID);
    }
    // REACTORS
    GridTerminalSystem.GetBlocksOfType &lt;IMyReactor&gt; (_blocks);
    for(int i = 0; i &lt; _blocks.Count; i++) {
        if(!_blocks [i].CustomName.Contains (holdID))
            _blocks [i].SetCustomName (_blocks [i].CustomName + " " + holdID);
    }
    // CONTAINERS
    GridTerminalSystem.GetBlocksOfType &lt;IMyCargoContainer&gt; (_blocks);
    for(int i = 0; i &lt; _blocks.Count; i++) {
        if(!_blocks [i].CustomName.Contains (depositID)) {
            if(!_blocks [i].CustomName.Contains (storageID))
                _blocks [i].SetCustomName (_blocks [i].CustomName + " " + storageID);
            if(!_blocks [i].CustomName.Contains (holdID))
                _blocks [i].SetCustomName (_blocks [i].CustomName + " " + holdID);
        }
    }
    // OXYGEN TANKS
    GridTerminalSystem.GetBlocksOfType &lt;IMyOxygenTank&gt; (_blocks);
    for(int i = 0; i &lt; _blocks.Count; i++) {
        if(!_blocks [i].CustomName.Contains (holdID))
            _blocks [i].SetCustomName (_blocks [i].CustomName + " " + holdID);
    }
    // OXYGEN GENERATORS
    GridTerminalSystem.GetBlocksOfType &lt;IMyOxygenGenerator&gt; (_blocks);
    for(int i = 0; i &lt; _blocks.Count; i++) {
        if(!_blocks [i].CustomName.Contains (holdID))
            _blocks [i].SetCustomName (_blocks [i].CustomName + " " + holdID);
    }
    Debug ("...complete.\n");
    return true;
}










