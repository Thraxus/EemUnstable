    if(!hideVersionInfoOnMenuScreens)
        MenuScreen.WritePublicText (version + " " + updated + tab + prefix + modes [mode] + suffix + "\n");
    else
        MenuScreen.WritePublicText ("\n");
    MenuScreen.WritePublicText (MenuScreen.GetPublicText () + GetMenuText (8));
    MenuScreen.WritePublicText (MenuScreen.GetPublicText () + tab + tab + prefix 
                                                                   + GetTranslatedSubtypeName (activeProduct) + suffix);
    MenuScreen.WritePublicText (MenuScreen.GetPublicText () + tab + tab + prefix  
                                                                   + "1 / " + displayFactor + suffix + "\n\n"); 
    

    List &lt;MAProduct&gt; _validProducts = new List &lt;MAProduct&gt; ();
    for (int i = 0; i &lt; products.Count; i++) {
        if(IsValid (products [i], mode) &amp;&amp; products [i].ID != currencyID &amp;&amp; products [i].SUBTYPE != currencySType) {
            _validProducts.Add (products [i]);
            // Debug (tab + "Added " + products [i].ID + " to valid Product List\n");
        }
    }

    _range = _validProducts.Count-1;
    MenuScreen.WritePublicText (MenuScreen.GetPublicText () + ShowValidItemList (_validProducts, activeIndex) + "\n\n");
    if (_validProducts.Count &gt; 0) {
        activeProduct = products.IndexOf (_validProducts [activeIndex]);
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + ShowActiveProductInfo (activeProduct) + "\n");
    }

    switch (GetInput ()) {
    case 0: if (activeIndex &gt; 0) { activeIndex--; } SetExternalData (); ShowMenu (); break;
    case 1:  if (activeIndex &lt; _range) { activeIndex++; } SetExternalData (); ShowMenu (); break;
    case 3000: SetStoredTradeValue (products [activeProduct]); ShowMenu (); break;
    case 4000: mode = 0; screenIndex = 4000; activeIndex = 0; SetExternalData (); ShowMenu (); break;
    default: break;
    }
}

