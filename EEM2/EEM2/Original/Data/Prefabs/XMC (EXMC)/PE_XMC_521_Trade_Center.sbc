        activeProduct = products.IndexOf (_validProducts [activeIndex]);
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + ShowActiveProductInfo (activeProduct) + "\n");
    }

    switch (GetInput ()) {
    case 0: if (activeIndex &gt; 0) { activeIndex--; } SetExternalData (); ShowMenu (); break;
    case 1:  if (activeIndex &lt; _range) { activeIndex++; } SetExternalData (); ShowMenu (); break;
    case 3000: SetStoredTradeValue (products [activeProduct]); ShowMenu (); break;
    case 4000: mode = 0; screenIndex = 4000; activeIndex = 0; SetExternalData (); ShowMenu (); break;
    default: break;
    }
}

