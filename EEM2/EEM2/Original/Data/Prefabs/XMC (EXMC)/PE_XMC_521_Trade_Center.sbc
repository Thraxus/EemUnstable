    MenuScreen.WritePublicText (MenuScreen.GetPublicText () + GetMenuText (7));

    switch (GetInput ()) { 
    case 0: if (activeIndex &gt; 0) { activeIndex--; } SetExternalData (); ShowMenu (); break;
