// &lt;&lt; AUTO INSTALLATION &gt;&gt;

bool Install () {
    List &lt;IMyTerminalBlock&gt; _blocks = new List &lt;IMyTerminalBlock&gt; ();
    // ASSEMBLERS
    GridTerminalSystem.GetBlocksOfType &lt;IMyAssembler&gt; (_blocks);
    for(int i = 0; i &lt; _blocks.Count; i++) {
        if(!_blocks [i].CustomName.Contains (productionID))
            _blocks [i].SetCustomName (_blocks [i].CustomName + " " + productionID);
    }
    // REFINERIES
    GridTerminalSystem.GetBlocksOfType &lt;IMyRefinery&gt; (_blocks);
    for(int i = 0; i &lt; _blocks.Count; i++) {
        if(!_blocks [i].CustomName.Contains (productionID))
            _blocks [i].SetCustomName (_blocks [i].CustomName + " " + productionID);
    }
    // SORTERS
    GridTerminalSystem.GetBlocksOfType &lt;IMyConveyorSorter&gt; (_blocks);
    for(int i = 0; i &lt; _blocks.Count; i++) {
        if(!_blocks [i].CustomName.Contains (holdID) &amp;&amp; !_blocks [i].CustomName.Contains (outputID))
            _blocks [i].SetCustomName (_blocks [i].CustomName + " " + holdID);
        if(!_blocks [i].CustomName.Contains (sorterID))
            _blocks [i].SetCustomName (_blocks [i].CustomName + " " + sorterID);
    }
    // REACTORS
    GridTerminalSystem.GetBlocksOfType &lt;IMyReactor&gt; (_blocks);
    for(int i = 0; i &lt; _blocks.Count; i++) {
        if(!_blocks [i].CustomName.Contains (holdID))
            _blocks [i].SetCustomName (_blocks [i].CustomName + " " + holdID);
    }
    // CONTAINERS
    GridTerminalSystem.GetBlocksOfType &lt;IMyCargoContainer&gt; (_blocks);
    for(int i = 0; i &lt; _blocks.Count; i++) {
        if(!_blocks [i].CustomName.Contains (depositID)) {
            if(!_blocks [i].CustomName.Contains (storageID))
                _blocks [i].SetCustomName (_blocks [i].CustomName + " " + storageID);
            if(!_blocks [i].CustomName.Contains (holdID))
                _blocks [i].SetCustomName (_blocks [i].CustomName + " " + holdID);
        }
    }
    // OXYGEN TANKS
    GridTerminalSystem.GetBlocksOfType &lt;IMyOxygenTank&gt; (_blocks);
    for(int i = 0; i &lt; _blocks.Count; i++) {
        if(!_blocks [i].CustomName.Contains (holdID))
            _blocks [i].SetCustomName (_blocks [i].CustomName + " " + holdID);
    }
    // OXYGEN GENERATORS
    GridTerminalSystem.GetBlocksOfType &lt;IMyOxygenGenerator&gt; (_blocks);
    for(int i = 0; i &lt; _blocks.Count; i++) {
        if(!_blocks [i].CustomName.Contains (holdID))
            _blocks [i].SetCustomName (_blocks [i].CustomName + " " + holdID);
    }
    Debug ("...complete.\n");
    return true;
}










// &lt;&lt; INITIALIZATION &gt;&gt;

bool InitBlocks () {
    int _requiredBlocks = 8;
    Storage = new List &lt;IMyInventory&gt; ();
    PlatformHold = new List &lt;IMyInventory&gt; ();
    InfoScreens = new List &lt;IMyTextPanel&gt; ();
    
    List &lt;IMyTerminalBlock&gt; _blocks = new List &lt;IMyTerminalBlock&gt; ();
    GridTerminalSystem.GetBlocksOfType &lt;IMyTerminalBlock&gt; (_blocks);

    Debug ("\n");

    for(int i = 0; i &lt; _blocks.Count; i++) {
        if (_blocks [i].CustomName.Contains (portID) &amp;&amp; !_blocks [i].CustomName.Contains (sorterID) 
                &amp;&amp; !_blocks [i].CustomName.Contains (holdID)) {
            // OUTPUT
            if (_blocks [i].CustomName.Contains (outputID)) {
                Output = (IMyInventoryOwner)_blocks [i];
                Debug (tab + "- found Output. \n");
                _requiredBlocks--;
            }
            // DEPOSIT
            if (_blocks [i].CustomName.Contains (depositID)) {
                Deposit = (IMyInventoryOwner)_blocks [i];
                Debug (tab + "- found Deposit. \n");
                _requiredBlocks--;
            }
            // BTN LEFT
            if (_blocks [i].CustomName.Contains (btn_left_ID)) {
                Btn_left = (IMyTimerBlock)_blocks [i];
                // Debug (tab + "- found Button Left. \n");
                _requiredBlocks--;
            }
            // BTN RIGHT
            if (_blocks [i].CustomName.Contains (btn_right_ID)) {
                Btn_right = (IMyTimerBlock)_blocks [i];
                // Debug (tab + "- found Button Right. \n");
                _requiredBlocks--;
            }
            // BTN OK
            if (_blocks [i].CustomName.Contains (btn_ok_ID)) {
                Btn_ok = (IMyTimerBlock)_blocks [i];
                // Debug (tab + "- found Button Ok. \n");
                _requiredBlocks--;
            }
            // BTN CANCEL
            if (_blocks [i].CustomName.Contains (btn_cancel_ID)) {
                Btn_cancel = (IMyTimerBlock)_blocks [i];
                // Debug (tab + "- found Button Cancel. \n");
                _requiredBlocks--;
            }
            // MENU SCREEN
            if (_blocks [i].CustomName.Contains (menuScreenID)) {
                MenuScreen = (IMyTextPanel)_blocks [i];
                MenuScreen.ShowPublicTextOnScreen ();
                // Debug (tab + "- found Menu Screen. \n");
                _requiredBlocks--;
            }
            // TERMINAL
            if (_blocks [i].CustomName.Contains (terminalID)) {
                Terminal = (IMyButtonPanel)_blocks [i];
                // Debug (tab + "- found Terminal. \n");
                _requiredBlocks--;
            }
            // INFO SCREENS
            if (_blocks [i].CustomName.Contains (infoScreenID)) {
                IMyTextPanel _panel = (IMyTextPanel)_blocks [i];
                InfoScreens.Add (_panel);
                _panel.ShowPublicTextOnScreen ();
            }
        }
        // NAV DATA
        if (_blocks [i].CustomName.Contains (navDataID)) {
            NavData = (IMyTextPanel)_blocks [i];
            Debug (tab + "- found Nav DATA Screen. \n");
            externalNavData = true;
        }
        // UPDATE TIMER 
        if (_blocks [i].CustomName.Contains (updateTimerID)) {
            UpdateTimer = (IMyTimerBlock)_blocks [i];
            Debug (tab + "- found Update Timer. \n");
            scrollingEnabled = true;
        }
        // STORAGE CONTAINERS (Only Add Containers the have at least 10% Free Storage Space)
        if (_blocks [i].CustomName.Contains (storageID) &amp;&amp; !IsFull (((IMyInventoryOwner)_blocks [i]).GetInventory (0)))
            Storage.Add (((IMyInventoryOwner)_blocks [i]).GetInventory (0));
        // PLATFORM HOLD CONTAINERS
        if(_blocks [i].CustomName.Contains (holdID))
            PlatformHold.Add (((IMyInventoryOwner)_blocks [i]).GetInventory (0));
        // STORAGE PRODUCTION
        if (_blocks [i].CustomName.Contains (productionID)) {
            PlatformHold.Add (((IMyInventoryOwner)_blocks [i]).GetInventory (0));
            PlatformHold.Add (((IMyInventoryOwner)_blocks [i]).GetInventory (1));
        }
    }
    if (Storage.Count &lt;= 0) {
        Debug ("...failed. No Storage has been found.\n");
        return false;
    }
    if (_requiredBlocks &lt;= 0) {
        Debug ("...complete. Storage: " + Storage.Count + " PlatformHold: " + PlatformHold.Count + "\n");
        return true;
    }
    else {
        Debug ("...failed. Not all required Blocks have been found!\n" + _requiredBlocks + " Blocks are missing.\n");
        return false;
    }
}

