// &lt;&lt; PROCESSING TRADE &gt;&gt;

bool EmptyDeposit (IMyInventoryOwner _deposit) {
    if(HasInput (_deposit.GetInventory (0))) {
        List &lt;IMyInventoryItem&gt; _items = _deposit.GetInventory (0).GetItems ();
        if(_items.Count &gt;= 0) {
            for(int i = 0; i &lt; _items.Count; i++) {
                // DEPOSIT IS NOT EMPTY
                if(_items [i].Content.ToString().Contains ("Ingot")) {
                    Debug ("[EmptyDeposit]: Detected " + _items [i].Amount + " Units of " + _items [i].Content.SubtypeName + " Ingots in Deposit.\n");
                }
                else if(_items [i].Content.ToString().Contains ("Ore")) {
                    Debug ("[EmptyDeposit]: Detected " + _items [i].Amount + " Units of " + _items [i].Content.SubtypeName + " Ore in Deposit.\n"); 
                }
                else
                    Debug ("[EmptyDeposit]: Detected " + _items [i].Amount + " Units of " + _items [i].Content.SubtypeName + " in Deposit.\n");

                // IS THE DETECTED ITEM VALID FOR TRADING WITH THIS PLATFORM?
                if(IsValid (_items [i], mode)) {
                    // IF MODE IS OFFER (Customer wants to BUY from Platform)
                    if(mode == 1) {
                        return OfferProduct (_items [i], i);
                    }
                    // IF MODE IS ACCEPT OR DEFAULT
                    else {
                        return AcceptItem (_items [i], i);
                    }
                }
                // ITEM IS NOT VALID FOR TRADING WITH THIS PLATFORM
                else {
                    Debug ("[EmptyDeposit]: Transferring " + (float)_items [i].Amount + "kg " + _items [i].Content.SubtypeName
                                + " back to Output.\n" + tab + "&lt;" + GetPlatformRoleID () + "&gt;"
                                + " does NOT Accept these Kind of Items.\n"); 
                    float _amount = DistributeToOutput (_items [i], i, (float)_items [i].Amount, Deposit.GetInventory (0));
                }
            }
        }
        return true;
    }
    return false;
}

// HANDLE SELL TO CUSTOMER PROCESS
bool OfferProduct (IMyInventoryItem _item, int _index) {
    MAProduct _activeProduct = products [currentSelection];
    Debug ("[OfferProduct]: Customer wants to buy " + _activeProduct.ID + " " + _activeProduct.SUBTYPE + " at a Trade Value of "
                + GetTradeValue (_activeProduct) + "kg per Unit.\n");
    float _tradeValue = GetTradeValue (_activeProduct);                 // The tradeValue of the desired Product per Unit
    float _productAmount = GetProductAmount (_activeProduct);   // The currently existing Amount of the desired Product
    float _transferAmount = (float)_item.Amount / _tradeValue;     // The amount of the desired Product the offered currency is worth
    float _currencyAmountLeft = (float)_item.Amount;                    // The amount of currency that has not yet been transferred.

    // GET PACKAGE SIZE OF DESIRED PRODUCT
    if(_transferAmount &gt; GetPackageSize (_activeProduct)) {
        Debug ("[OfferProduct]:" + _transferAmount + "kg is too large, reducing PackagSize to " + GetPackageSize (_activeProduct) 
                    + "kg.\n");
        _transferAmount = GetPackageSize (_activeProduct);
    }

    float _stackTradeValue = _transferAmount * _tradeValue;         // The Amount of Currency that will be transferred to platform Storage
    float _unTransferredProduct = _productAmount;

    Debug ("[OfferProduct]: Customer offered " + (float)_item.Amount + "kg of Currency. That would be " 
                + ((float)_item.Amount / _tradeValue) + "kg of " + _activeProduct.ID + " " + _activeProduct.SUBTYPE + ".\n");

    if(_transferAmount &lt;= _productAmount) {
        // DISTRIBUTE TRADE GOODS FROM PLATFORM STORAGE TO OUTPUT FIRST
        _unTransferredProduct = DistributeToOutput (_activeProduct.ID, _activeProduct.SUBTYPE, _transferAmount);
        Debug ("[OfferProduct]: " + _transferAmount + "kg of " + _activeProduct.ID + " "
                    + _activeProduct.SUBTYPE + " have been transferred to Output.\n");

        // DISTRIBUTE CURRENCY FIRST FROM DEPOSIT TO PLATFORM STORAGE
        _currencyAmountLeft = DistributeToStorage (_item, _index, _stackTradeValue, Deposit.GetInventory (0));
        Debug ("[OfferProduct]: " + _stackTradeValue + "kg of " + currencyID + " " + currencySType
                    + " have been transferred to Storage.\n");
        return true;
    }
    else {
        Debug ("[OfferProduct]: There are only " + _productAmount + "kg " + _activeProduct.ID + " " 
            + _activeProduct.SUBTYPE + " left.\n" + tab + "Customer has offered more Currency. Transferring Currency back to Output.\n");
        _currencyAmountLeft = DistributeToOutput (_item, _index, (float)_item.Amount, Deposit.GetInventory (0));
        return true;
    }
    Debug ("[OfferProduct]: Error, this point should never be reached!\n" + tab + "Something went wrong during Transfer.\n");
    return false;
}

// HANDLE BUY FROM CUSTOMER PROCESS
bool AcceptItem (IMyInventoryItem _item, int _index) {
    float _transferAmount = GetPackageSize (_item);
    int _productIndex = GetProductIndexByItem (_item);

    // IF PRODUCT IS NOT THE ONE TRADED THE CALL BEFORE
    if(_productIndex != currentSelection) {
        // SET THE TRADE VALUE OF THE CURRENT ITEM FOR THE HOLE TRADE
        Debug ("[AcceptItem]: Active Product is now " + products [_productIndex].ID + " " + products [_productIndex].SUBTYPE + "\n");
        currentSelection = _productIndex;
        SetStoredTradeValue (_item);
    }

    // CALCULATE TRADE VALUE OF CURRENT ITEM STACK
    float _stackTradeValue = storedTradeValue * _transferAmount;
    float _amountLeft = 0;
    Debug ("[AcceptItem]: " + _transferAmount + " Units of " + _item.Content.SubtypeName + " have a TradeValue of "
                + _stackTradeValue.ToString ("0.00") + "kg " + currencyID + " " + currencySType + ".\n");

    // IF THERE IS ENOUGH CURRENCY TO BUY ITEMS FROM DEPOSIT AND MODE IS NOT DEFAULT
    if(_stackTradeValue &lt;= GetCurrencyAmount () &amp;&amp; mode != 0) {
        // DISTRIBUTE CURRENCY FIRST FROM PLATFORM STORAGE -&gt; OUTPUT
        DistributeToOutput (currencyID, currencySType, _stackTradeValue);

        // DISTRIBUTE TRADE GOODS FROM DEPOSIT -&gt; PLATFORM STORAGE
        _amountLeft = DistributeToStorage (_item, _index, _transferAmount, Deposit.GetInventory (0));
        Debug ("[AcceptItem]: Transferred " + _transferAmount + "kg " + _item.Content.SubtypeName
                    + " to Storage. There are " + _amountLeft + "kg left in Deposit.\n");
        return true;
    }
    // MODE IS DEFAULT OR NOT ENOUGH CURRENCY LEFT, TRANSFER DIRECTLY DEPOSIT -&gt; OUTPUT
    else if(mode == 0) {
        Debug ("[AcceptItem]: Transferring " + _transferAmount + "kg " + _item.Content.SubtypeName + " to Output.\n"
                + tab + "Platform does NOT Accept Items in " + modes [mode] + " Mode.\n");
        _amountLeft = DistributeToOutput (_item, _index, _transferAmount, Deposit.GetInventory (0));
        return true;
    }
    else {
        Debug ("[AcceptItem]: Not enough Currency left to pay: " + GetCurrencyAmount () + "\n" + tab
                + "Transferring " + _transferAmount + "kg " + _item.Content.SubtypeName + " back to Output.\n");
        _amountLeft = DistributeToOutput (_item, _index, _transferAmount, Deposit.GetInventory (0));
        return true;
    }
    Debug ("[AcceptItem]: Error, this point should never be reached!\n" + tab + "Something went wrong during Transfer.\n");
    return false;
}

// DISTRIBUTE A PASSED-IN _transferAmount OF _item TO PLATFORM STORAGE
float DistributeToStorage (IMyInventoryItem _item, int _index, float _transferAmount, IMyInventory _sourceInv) {
    for(int i = 0; i &lt; Storage.Count; i++) {
        if(HasEnoughRoom (Storage [i], _transferAmount)) {
            Debug ("[DistributeToStorage]: Transferring " + _transferAmount + "l to Storage\n");
            _sourceInv.TransferItemTo (Storage [i], _index, null, true, (VRage.MyFixedPoint)_transferAmount);
            break;
        }
    }
    return (float)_item.Amount - _transferAmount;
}

// DISTRIBUTE A PASSED-IN _transferAmount OF  _item TO OUTPUT INVENTORY
float DistributeToOutput (IMyInventoryItem _item, int _index, float _transferAmount, IMyInventory _sourceInv) {
    float _notTransferred = _transferAmount;
    IMyInventory _destInv = Output.GetInventory (0);
    // IF THERE IS NOT ENOUGH ROOM IN THE OUTPUT, DECREASE PACKAGE SIZE
    if(GetEmptyStorageSpace (_destInv) &lt; _transferAmount) {
        _transferAmount = GetEmptyStorageSpace (_destInv);
        _notTransferred -= GetEmptyStorageSpace (_destInv);
    }
    Debug ("[DistributeToOutput]: Transferring " + _transferAmount + "kg of " + _item.Content.SubtypeName + " to Output\n");
    _sourceInv.TransferItemTo (_destInv, _index, null, true, (VRage.MyFixedPoint)_transferAmount);
    
    return _notTransferred;
}

// DISTRIBUTE A PASSED-IN _transferAmount OF  _productID &amp; _productSType TO OUTPUT INVENTORY
float DistributeToOutput (string _productID, string _productSType, float _transferAmount) {
    IMyInventory _destInv = Output.GetInventory (0);
    float _amountLeft = _transferAmount;
    // FIND THE ITEM INSIDE STORAGE
    for(int i = 0; i &lt; Storage.Count; i++) {
        List &lt;IMyInventoryItem&gt; _items = Storage [i].GetItems ();
        if(_items.Count &gt; 0) {
            for(int a = 0; a &lt; _items.Count; a++) {
                if(_items [a].Content.SubtypeName.Contains (_productID) &amp;&amp; _items [a].Content.ToString ().Contains (_productSType)) {
                    _amountLeft = _transferAmount - DistributeToOutput (_items [a], a, _transferAmount, Storage [i]);
                }
                if(_amountLeft &lt;= 0)
                    break;
            }
            if(_amountLeft &lt;= 0) 
               break;
        }
    }
    return _amountLeft;
}













