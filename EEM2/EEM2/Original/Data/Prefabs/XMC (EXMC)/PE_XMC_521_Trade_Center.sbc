float GetTradeValue (MAProduct _product) {
    float _value = _product.BASEVALUE;
    float _amount = GetProductAmount (_product);

    if(currencyID == _product.ID &amp;&amp; currencySType == _product.SUBTYPE) {
        return 1;
    }

    // IF MODE IS "OFFER"
    if(mode == 1) {
        if(_amount &lt; _product.MINAMOUNT) {
            _value *= 1.39f;
        }
        else if (_amount &gt; _product.MINAMOUNT &amp;&amp; _amount &lt; _product.MINAMOUNT * 3) {
            _value *= 1.05f;
        }
        else if (_amount &gt; _product.MINAMOUNT * 3 &amp;&amp; _amount &lt; _product.MINAMOUNT * 6) {
             _value *= 0.99f;
        }
        else if (_amount &gt; _product.MINAMOUNT * 6) {
             _value *= 0.89f;
        }
    }
    // IF MODE IS EITHER "ACCEPT" OR "DEFAULT"
    else {
        if(_amount &lt; _product.MINAMOUNT) {
            _value *= 1.35f;
        }
        else if (_amount &gt; _product.MINAMOUNT &amp;&amp; _amount &lt; _product.MINAMOUNT * 3) {
            _value *= 1.00f;
        }
        else if (_amount &gt; _product.MINAMOUNT * 3 &amp;&amp; _amount &lt; _product.MINAMOUNT * 6) {
             _value *= 0.95f;
        }
        else if (_amount &gt; _product.MINAMOUNT * 6) {
             _value *= 0.85f;
        }
    }
    return _value;
}

string GetPriceIndicator (MAProduct _product) {
    float _factor = 100 / _product.BASEVALUE * GetTradeValue (_product);
    if(_factor &gt; 1)
        return high + empty + _factor.ToString ("0.00") + "%" + GetMenuText (13);
    if(_factor &lt; 1)
        return low + empty + _factor.ToString ("0.00") + "%" + GetMenuText (13);
    return moderate + empty + _factor.ToString ("0.00") + "%" + GetMenuText (13);
}

