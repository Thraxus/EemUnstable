// THE ITEM LIST ON BOTH OFFERS AND AQUISITIONS
string ShowValidItemList (List &lt;MAProduct&gt; _validProducts, int _activeIndex) {
    string _list = "";
    int _range = 7;
    int _currentSelection =  0;

    if (_validProducts.Count &lt;= 0) {
        _list = tab + GetMenuText (10);
        if(mode == 2)
            _list = tab + GetMenuText (11);
    }
    else {
        _currentSelection = _validProducts.IndexOf (products [currentSelection]);
        for (int i = 0; i &lt; _validProducts.Count; i++) {
            if ((i &lt; _range &amp;&amp; _activeIndex &lt;= _range/2) || (i &gt;= _activeIndex - (_range/2) &amp;&amp; i &lt;= _activeIndex + (_range/2)) 
                    || (i &gt;= (_validProducts.Count - _range) &amp;&amp;  i &lt;= _activeIndex) ) {
                string _price = (GetTradeValue(products[products.IndexOf(_validProducts [i])])*displayFactor).ToString (GetListFormat ());

                if(i == _activeIndex &amp;&amp; i != _currentSelection) {
                    string _first = tab + empty + prefix + GetTranslatedProductName (products.IndexOf(_validProducts [i]));
                    string _last = _price + suffix + empty + "\n";
                    _list += _first + GetPlaceHolders (_first + _last) + _last;
                }
                else if(i == _activeIndex &amp;&amp; i == _currentSelection) {
                    string _first = tab + activePrefix + prefix + GetTranslatedProductName (products.IndexOf(_validProducts [i]));
                    string _last = _price + suffix + activeSuffix +  "\n";
                    _list += _first + GetPlaceHolders (_first + _last) + _last;
                }
                else if(i != _activeIndex &amp;&amp; i == _currentSelection) {
                    string _first = tab + activePrefix + empty + GetTranslatedProductName (products.IndexOf(_validProducts [i]));
                    string _last = _price + empty + activeSuffix +  "\n";
                    _list += _first + GetPlaceHolders (_first + _last) + _last;
                }
                else {
                    string _first = tab + empty + empty + GetTranslatedProductName (products.IndexOf(_validProducts [i]));
                    string _last = _price + empty + empty +  "\n";
                    _list += _first + GetPlaceHolders (_first + _last) + _last;
                }
            }
        }
    }
    for(int i = 0; i &lt; _range - _validProducts.Count; i++) {
        _list += "\n";
    }
    return _list;
}


// THE PRODUCT INFO BELOW THE ITEM LIST
string ShowActiveProductInfo (int _index) {
    string _info = "";
    MAProduct _product = products [_index];
    _info += tab + "----------------------------------------------------------------------------\n";
    string _first = tab + tab + GetTranslatedProductName (_index);
    string _last =  GetProductAmount (_product).ToString ("0,000") + "kg " + GetMenuText (12);
    _info += _first + GetPlaceHolders (_first + GetMenuText (12)) + _last;
    _info += tab + tab + GetPriceIndicator (_product);
    _info += tab + "----------------------------------------------------------------------------\n";
    return _info;
}

string GetPlaceHolders (string _input) {
    int _amount = _input.Length;
    int _outputLength = 0;
    string _placeHolders = "";

    int _cntSmalls = 0;
    int _cntMediums = 0;
    int _cntLarges = 0;

// .
    _cntSmalls += CountChar ('1', _input);
    _cntSmalls += CountChar ('[', _input);
    _cntSmalls += CountChar (' ', _input);
    _cntSmalls += CountChar ('f', _input);
    _cntSmalls += CountChar ('I', _input); 
    _cntSmalls += CountChar ('i', _input);
    _cntSmalls += CountChar ('j', _input);
    _cntSmalls += CountChar ('l', _input);
    _cntSmalls += CountChar ('r', _input);
    _cntSmalls += CountChar ('t', _input);
// ...
    _cntLarges += CountChar ('M', _input);
    _cntLarges += CountChar ('m', _input);
    _cntLarges += CountChar ('W', _input);
    _cntLarges += CountChar ('w', _input);
// ..
    _cntMediums = _amount - _cntSmalls - _cntLarges;

    _outputLength = _cntSmalls + (_cntMediums * 2) + (_cntLarges * 3);
    _amount = lineWidth - _outputLength;

    for(int i = 0; i &lt; _amount; i++) {
        _placeHolders += placeHolder;
    }

    return _placeHolders;
}

int CountChar (char _x, string _input) {
    int _amount = 0;
    char [] _temp = _input.ToCharArray ();
    for(int i = 0; i &lt; _temp.Length; i++) {
        if(_temp [i] == _x)
            _amount++;
    }
    return _amount;
}





