float GetPackageSize (IMyInventoryItem _item) {
    // GET VOLUME OF ITEM STACK FOUND @ DEPOSIT TO CHECK IF IT FITS MAX PACKAGESIZE
    float _volPerUnit = GetItemVolume (_item);
    float _transferVolume = _volPerUnit * (float)_item.Amount;
    // Debug ("[GetPackageSize]: VolPerUnit: " + _volPerUnit + "l\n");
    // Debug ("[GetPackageSize]: Calculated TVol: " + _transferVolume + "l\n");
    if(_transferVolume &gt; packageSize) {
        _transferVolume = packageSize;
        // Debug ("[GetPackageSize]: Reduced packageSize to " + _transferVolume + "l\n");
    }
    float _transferAmount = _transferVolume / _volPerUnit;
    // Debug ("[GetPackageSize]: Calculated TAmount: " + _transferAmount + "kg\n");
    // CALCULATE CORRECT PACKAGE
   return _transferAmount;
}

