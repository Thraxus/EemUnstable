// THE MAIN MENU
void ShowMainMenu () {
    int _range  = 2;
    if(!hideVersionInfoOnMenuScreens)
        MenuScreen.WritePublicText (version + " " + updated + tab + prefix + modes [mode] + suffix + "\n");
    else
        MenuScreen.WritePublicText ("\n");
    
    if(activeIndex == 0) {
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + "\n\n" + GetMenuText (5) + "\n");
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + GetMenuText (16) + "\n");
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + GetMenuText (1));
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + GetMenuText (4) + "\n\n\n");
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + tab + GetLanguageBtnDeselected ());
    }
    else if (activeIndex == 1) {
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + "\n\n"+ GetMenuText (5) + "\n");
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + GetMenuText (16) + "\n");
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + GetMenuText (2));
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + GetMenuText (3) + "\n\n\n");
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + tab + GetLanguageBtnDeselected ());
    }
    else if (activeIndex == 2) {
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + "\n\n"+ GetMenuText (5) + "\n");
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + GetMenuText (16) + "\n");
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + GetMenuText (2));
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + GetMenuText (4) + "\n\n\n");
        MenuScreen.WritePublicText (MenuScreen.GetPublicText () + tab + GetLanguageBtnSelected ());
    }
    else {
        MenuScreen.WritePublicText (MenuScreen.GetPublicText ());
    }
    
    MenuScreen.WritePublicText (MenuScreen.GetPublicText () + "\n" + GetMenuText (6));
    MenuScreen.WritePublicText (MenuScreen.GetPublicText () + GetMenuText (7));

    switch (GetInput ()) { 
    case 0: if (activeIndex &gt; 0) { activeIndex--; } SetExternalData (); ShowMenu (); break;
    case 1:  if (activeIndex &lt; _range) { activeIndex++; } SetExternalData (); ShowMenu (); break;
    case 3000:
    if(activeIndex &lt; 2) {
        screenIndex = activeIndex;
        activeIndex = 0;
    }
    else {
