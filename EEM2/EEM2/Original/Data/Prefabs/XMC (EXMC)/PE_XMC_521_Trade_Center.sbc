// &lt;&lt; IS / HAS &gt;&gt;

bool IsValid (IMyInventoryItem _item) {
    return IsValid (products[GetProductIndexByItem (_item)], mode);
}

bool IsValid (MAProduct _product) {
    return IsValid (_product, mode);
}

bool IsValid (IMyInventoryItem _item, int _mode) {
    return IsValid (products[GetProductIndexByItem (_item)], _mode);
}

bool IsValid (MAProduct _product, int _mode) {
    string _sType = _product.SUBTYPE;
    // IF MODE IS "OFFER"
    if(_mode == 1) {
        if(_product.ID == currencyID &amp;&amp; _sType == currencySType)
            return true;
        // ONLY SHOW PRODUCTS WITH AVAILABLE AMOUNT
        if(GetProductAmount (_product) &gt; 1) {
            switch (_sType) {
            case "Ore": return platformRoles [platformRoleID].OFFERORE;
            case "Ingot": return platformRoles [platformRoleID].OFFERINGOTS;
            case "Component": return platformRoles [platformRoleID].OFFERCOMP;
            case "AmmoMagazine": return platformRoles [platformRoleID].OFFERAMMO;
            case "PhysicalGunObject": return platformRoles [platformRoleID].OFFERTOOLS;
            default: return false;
            }
        }
        else
            return false;
    }
    // IF MODE IS "ACCEPT" or "DEFAULT"
    else {
        // We can't buy unless there is currency aboard the platform!
        if(GetCurrencyAmount () &gt; 1) {
            switch (_sType) {
            case "Ore": return platformRoles [platformRoleID].ACCEPTORE;
            case "Ingot": return platformRoles [platformRoleID].ACCEPTINGOTS;
            case "Component": return platformRoles [platformRoleID].ACCEPTCOMP;
            case "AmmoMagazine": return platformRoles [platformRoleID].ACCEPTAMMO;
            case "PhysicalGunObject": return platformRoles [platformRoleID].ACCEPTTOOLS;
            default: return false;
            }
        }
        else
            return false;
    }
}
 
bool IsFull (IMyInventory _inv) { 
    if (100 / (float)_inv.MaxVolume * (float)_inv.CurrentVolume &gt; 90f)
        return true;
    else 
        return false; 
}

bool HasEnoughRoom (IMyInventory _inv, float _amount) {
    if ((float)_inv.MaxVolume - (float)_inv.CurrentVolume &gt;= _amount)
        return true;
    else
        return false;
}

bool HasInput (IMyInventory _deposit) {
    List &lt;IMyInventoryItem&gt; _items = _deposit.GetItems ();
    if(_items.Count &gt; 0)
        return true;
    else
        return false;
}













