// TRANSFER PROCESSING  
  
void EmptyDeposit () {  
if (Deposit != null) {  
List &lt;IMyInventoryItem&gt; _itemsInDeposit = Deposit.GetItems (); 
for (int a = 0;a &lt; _itemsInDeposit.Count;a++) {  
bool _accepted = false; 
  
for (int i = 0;i &lt; validAquires.Count;i++) {  
// IF ITEM IS ACCEPTED  
if (_itemsInDeposit [a].Content.SubtypeName == validAquires [i].SubTypeID  
&amp;&amp; _itemsInDeposit [a].Content.ToString ().Contains (validAquires [i].ItemClass)) {  
Debug ("Item " + _itemsInDeposit [a].Content.SubtypeName + " has been accepted.", true); 
customerCredit += validAquires [i].AquireValue * (double)_itemsInDeposit [a].Amount; 
if (!TransferToStorage (_itemsInDeposit [a], a)) {  
Debug ("Could not transfer " + _itemsInDeposit [a].Content.SubtypeName + " to Storage. No Storage Space found!", true); 
}  
_accepted = true; 
break; 
}  
}  
  
if (!_accepted) {  
Debug ("Item " + _itemsInDeposit [a].Content.SubtypeName + " has been declined.", true); 
if (!TransferToOutput (Deposit, _itemsInDeposit [a], a)) {  
Debug ("Could not transfer " + _itemsInDeposit [a].Content.SubtypeName + " to Output. Not found!", true); 
}  
}  
}  
}else  
scriptEnabled = false; 
}  
  
void EmptyCollectors () {  
for (int i = 0;i &lt; collectors.Count;i++) {  
if (collectors [i] != null) {  
List &lt;IMyInventoryItem&gt; _items = collectors [i].GetItems (); 
for (int a = 0;a &lt; _items.Count;a++) {  
if (!TransferToDeposit (collectors [i], _items [a], a)) {  
Debug ("Could not transfer " + _items [a].Content.SubtypeName + " to Deposit. Not found!", true); 
}  
}  
}else {  
currentState = 1; 
}  
}  
}  
  
bool TransferToDeposit (IMyInventory _source, IMyInventoryItem _item, int _invIndex) {  
if (Deposit != null) {  
_source.TransferItemTo (Deposit, _invIndex, null, true, _item.Amount); 
return true; 
}else {  
return false; 
}  
}  
  
bool TransferToStorage (IMyInventoryItem _item, int _invIndex) {  
if (Deposit != null) {  
IMyInventory _source = Deposit; 
IMyInventory _dest = GetEmptyStorageSpace (_item); 
if (_dest != null) {  
_source.TransferItemTo (_dest, _invIndex, null, true, _item.Amount); 
return true; 
}else {  
return false; 
}  
}else {  
return false; 
}  
}  
  
IMyInventory GetEmptyStorageSpace (IMyInventoryItem _item) {  
 for (int i = 0;i &lt; storage.Count;i++) {  
if (storage [i] != null) {  
if (storage [i] != Output &amp;&amp; storage [i] != Deposit) {  
 if (storage [i].MaxVolume - storage [i].CurrentVolume &gt;= Deposit.CurrentVolume) {  
return storage [i]; 
 }  
} else {  
 currentState = 1; 
}  
}  
 }  
 return null; 
}  
  
bool TransferToOutput (IMyInventory _source, IMyInventoryItem _item, int _invIndex) {  
if (Output != null) {  
_source.TransferItemTo (Output, _invIndex, null, true, _item.Amount); 
return true; 
}  
else  
return false; 
}  
  
bool TransferToOutput (IMyInventory _source, IMyInventoryItem _item, int _invIndex, double _amount) {  
if (Output != null) {  
_source.TransferItemTo (Output, _invIndex, null, true, (VRage.MyFixedPoint)_amount); 
return true; 
}  
else  
return false; 
}  
  
void AbortTrade () {  
for (int i = 0;i &lt; storage.Count;i++) {  
if (storage [i] != null) {  
List &lt;IMyInventoryItem&gt; _items = storage [i].GetItems (); 
for (int a = 0;a &lt; _items.Count;a++) {  
if (_items [a].Content.SubtypeName == CURRENCY_ID  
&amp;&amp; _items [a].Content.ToString ().Contains (CURRENCY_CLASS)) {  
if ((double)_items [a].Amount &gt;= customerCredit) {  
if (!TransferToOutput (storage [i], _items [a], a, customerCredit)) {  
Debug ("AbortTrade: Could not transfer " + customerCredit + "x " + _items [a].Content.SubtypeName + " to Output.", true); 
}  
else {  
customerCredit = 0; 
break; 
}  
}else {  
double _amount = (double)_items [a].Amount; 
if (!TransferToOutput (storage [i], _items [a], a, _amount)) {  
Debug ("AbortTrade: Could not transfer " + _amount + "x " + _items [a].Content.SubtypeName + " to Output.", true); 
}  
else  
customerCredit -= _amount; 
}  
}  
}  
}else {  
currentState = 1; 
}  
}  
}  
  
void Buy () {  
  
SIOSProduct _selectedProduct = products [selectedProductIndex]; 
Debug ("Buy: Selected Product: " + _selectedProduct.ItemNames [languageID], true); 
  
// TOOLs &amp; Containers are only sold per Unit  
  
if (_selectedProduct.ItemClass == itemClasses [6] || _selectedProduct.ItemClass.Contains(itemClasses [4])) {  
packageSize = 1; 
Debug ("Buy: Reduced packageSize to 1", true); 
}  
  
// Get overallprice  
  
double _overallPrice = _selectedProduct.OfferValue * packageSize; 
  
// Check if customer credit fits overallprice  
  
if (customerCredit &lt; _overallPrice) {  
if (packageSize == 100) {  
packageSize = 10; 
Debug ("Buy: Reduced packageSize to 10", true); 
_overallPrice = _selectedProduct.OfferValue * packageSize; 
}  
  
// Check customer credit again against overall price  
  
if (customerCredit &lt; _overallPrice) {  
packageSize = _selectedProduct.MinPackageSize; 
Debug ("Buy: Reduced packageSize to Min", true); 
_overallPrice = _selectedProduct.OfferValue * packageSize; 
}  
}  
  
// If not enough money abort trade at this point  
  
if (customerCredit &gt;= _overallPrice) {  
Debug ("Buy: Trade of " + _selectedProduct.SubTypeID + " possible.", true); 
double _amountToTransfer = packageSize; 
  
for (int i = 0;i &lt; storage.Count;i++) {  
if (storage [i] != null) {  
List &lt;IMyInventoryItem&gt; _items = storage [i].GetItems (); 
for (int a = 0;a &lt; _items.Count;a++) {  
if (_items [a].Content.SubtypeName == _selectedProduct.SubTypeID  
&amp;&amp; _items [a].Content.ToString ().Contains (_selectedProduct.ItemClass)) {  
Debug ("Buy: Found item.", true); 
if ((double)_items [a].Amount &gt;= _amountToTransfer) {  
customerCredit -= _selectedProduct.OfferValue * _amountToTransfer; 
if (!TransferToOutput (storage [i], _items [a], a, _amountToTransfer)) {  
Debug ("Buy: Could not transfer " + _amountToTransfer.ToString () + "x " + _items [a].Content.SubtypeName + " to Output.", true); 
waitingProducts.Add (_selectedProduct); 
waitingProductsAmount.Add (_amountToTransfer); 
}else {  
Debug ("Buy: Trade complete.", true); 
_amountToTransfer = 0; 
  
// Update all lists to avoid index out of bounds when seeling the last list entry item  
if (products [selectedProductIndex].CurrentAmount &lt; 1) {  
productSelected = false; 
selectedProductIndex = 0; 
hoveredProductIndex--; 
LoadItems (); 
}  
  
// Cut loop, no need to run again, for all items have been transferred  
break; 
}  
}else {  
double _transferredAmount = (double)_items [a].Amount; 
customerCredit -= _selectedProduct.OfferValue * _transferredAmount; 
if (!TransferToOutput (storage [i], _items [a], a, _transferredAmount)) {  
waitingProducts.Add (_selectedProduct); 
waitingProductsAmount.Add (_transferredAmount); 
Debug ("Buy: Could not transfer " + _transferredAmount.ToString () + "x " + _items [a].Content.SubtypeName + " to Output.", true); 
}else {  
_amountToTransfer -= _transferredAmount; 
Debug ("Buy: Trade partially completed, " + _amountToTransfer + " left to be transferred.", true); 
// Rewrite List after 1 Object has been moved to another Inventory  
_items = storage [i].GetItems (); 
// Restart list run for list count has changed.  
a = 0; 
}  
}  
UpdateProductAmounts (); 
LoadValidItems (); 
}  
}  
  
// Break if transfer is completed. No need to search the rest of possible inventories  
if (_amountToTransfer &lt;= 0)  
break; 
  
}else {  
currentState = 1; 
}  
}  
}else {  
Debug ("Buy: Not enough money: " + (_overallPrice - customerCredit).ToString () + EMPTY + CURRENCY_UNIT + " missing.", true); 
}  
  
// Update Product List screen to new list and indizes  
  
productListScreen = DoProductListScreen ("Offer"); 
}  
  
void TransferWaitingProducts () {  
for (int y = 0;y &lt; waitingProducts.Count;y++) {  
  
for (int i = 0;i &lt; storage.Count;i++) {  
if (storage [i] != null) {  
List &lt;IMyInventoryItem&gt; _items = storage [i].GetItems (); 
for (int a = 0;a &lt; _items.Count;a++) {  
if (_items [a].Content.SubtypeName == waitingProducts [y].SubTypeID  
&amp;&amp; _items [a].Content.ToString ().Contains (waitingProducts [y].ItemClass)) {  
if ((double)_items [a].Amount &gt;= waitingProductsAmount [y]) {  
if (!TransferToOutput (storage [i], _items [a], a, waitingProductsAmount [y])) {  
Debug ("WaitingProducts: Could not transfer " + waitingProductsAmount [y].ToString () + "x " + _items [a].Content.SubtypeName + " to Output.", true); 
}else {  
waitingProducts.RemoveAt (y); 
waitingProductsAmount.RemoveAt (y); 
y--; 
waitingProductsAmount [y] = 0; 
break; 
}  
}else {  
double _transferredAmount = (double)_items [a].Amount; 
if (!TransferToOutput (storage [i], _items [a], a, _transferredAmount)) {  
Debug ("WaitingProducts: Could not transfer " + _transferredAmount.ToString () + "x " + _items [a].Content.SubtypeName + " to Output.", true); 
}else {  
waitingProductsAmount [y] -= _transferredAmount; 
}  
}  
}  
}  
}else {  
currentState = 1; 
}  
}  
}  
}  
  
