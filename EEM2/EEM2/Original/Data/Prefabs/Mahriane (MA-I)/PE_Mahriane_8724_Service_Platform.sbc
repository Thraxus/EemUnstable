void Init () {  
debugMessages = new List &lt;string&gt; (); 
  
Clear (); 
  
Echo ("Initializing"); 
stateNames = new List&lt;string&gt; (); 
stateNames.Add ("Not initialized"); 
stateNames.Add ("Booting Lists"); 
stateNames.Add ("Getting Blocks"); 
stateNames.Add ("Getting Items"); 
stateNames.Add ("Ready"); 
  
itemClasses = new List&lt;string&gt; (); 
itemClasses.Add ("Ice"); 
itemClasses.Add ("MyObjectBuilder_Ore"); 
itemClasses.Add ("MyObjectBuilder_Ingot"); 
itemClasses.Add ("MyObjectBuilder_Component"); 
itemClasses.Add ("ContainerObject"); 
itemClasses.Add ("MyObjectBuilder_AmmoMagazine"); 
itemClasses.Add ("MyObjectBuilder_PhysicalGunObject"); 
  
InitPanels (); 
InitProducts (); 
InitRoles (); 
InitMenuText (); 
  
// GET CORE &amp; LOGON //   
  
List &lt;IMyTerminalBlock&gt; _blocks = new List &lt;IMyTerminalBlock&gt; (); 
GridTerminalSystem.SearchBlocksOfName (CORE_ID, _blocks); 
  
if (_blocks.Count &gt; 0) {  
 Core = (IMyProgrammableBlock)_blocks [0]; 
 Logon (); 
}  
  
currentState++; 
}  
  
void Logon () {  
Core.TryRun (API_APP_LOGON + EMPTY + APP_ID + TRADE_PORT_ID); 
Debug ("Found Core. Logged On.", true); 
}  
  
void Logoff () {  
Core.TryRun (API_APP_LOGOFF + EMPTY + APP_ID + TRADE_PORT_ID); 
Debug ("Logged Off.", true); 
}  
  
void StateUpdate () {  
Core.TryRun (API_APP_STATE_UPDATE + EMPTY + APP_ID + TRADE_PORT_ID + ORDER_SEPERATOR + stateNames [currentState]); 
Debug ("Sending state update to Core.", true); 
}  
  
void InitPanels () {  
Echo ("Initializing Panels"); 
List &lt;IMyTerminalBlock&gt; _blocks = new List&lt;IMyTerminalBlock&gt; (); 
GridTerminalSystem.SearchBlocksOfName (BID_TEXTPANEL, _blocks); 
for (int i = 0;i &lt; _blocks.Count;i++) {  
string _name = _blocks [i].CustomName; 
if (_name.Contains (platformID)) {  
IMyTextPanel _panel = (IMyTextPanel)_blocks [i]; 
string _title = _panel.GetPublicTitle (); 
if (_name.Contains (TRADE_PORT_ID)) {  
if (_title == TID_TRADE || _title.Contains (TID_TRADE)) {  
if (autoColorDisplay) {  
_panel.SetValueColor ("BackgroundColor", tradeBackground); 
_panel.SetValueColor ("FontColor", tradeFont); 
_panel.SetValueFloat ("FontSize", tradeFontSize); 
}  
TradeDisplay = _panel; 
}  
else if (_title == TID_DEBUG || _title.Contains (TID_DEBUG)) {  
if (autoColorDisplay) {  
_panel.SetValueColor ("BackgroundColor", debugBackground); 
_panel.SetValueColor ("FontColor", debugFont); 
_panel.SetValueFloat ("FontSize", debugFontSize); 
}  
Debugger = _panel; 
}  
else if (_title == TID_TUTORIAL || _title.Contains (TID_TUTORIAL)) {  
if (autoColorDisplay) {  
_panel.SetValueColor ("BackgroundColor", tradeBackground); 
_panel.SetValueColor ("FontColor", tradeFont); 
_panel.SetValueFloat ("FontSize", tradeFontSize); 
}  
TutorialDisplay = _panel; 
}  
}  
else if (_title == TID_CONFIG) {  
Config = _panel; 
useExternalConfig = true; 
LoadExternalConfigData (); 
}  
}  
}  
