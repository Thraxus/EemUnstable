// MAIN METHOD  
  
void Main (string argument) {  
if (currentState &lt; 1) {  
Init (); 
}  
else {  
EchoOut (); 
if (currentState &lt; 4) {  
Boot (); 
}  
else {  
if (scriptEnabled) {  
// GENERAL PROCESSING //  
  
if (argument.Trim ().Length &gt; 0) {  
if (!ProcessArgument (argument)) {  
Echo ("Recieved invalid argument: " + argument); 
}  
}  
 
// Check if this is a retail port 
if (noShipTrade) { 
 EnablePortBlocks (); 
 tradeEnabled = true; 
} else if (!tradeEnabled) { 
 DisablePortBlocks (); 
} 
 
// ENDOF GENERAL PROCESSING //  
}else  
currentState = 1; 
// DisplayOnScreen is called even when program is not yet booted or not yet enabled.  
// It has to be called before the Run () method to update screen on Button Input  
// even if the run has to be ignored! NOTE: Input ignores the next Run ()!  
DisplayOnScreen (); 
  
// Update Script Info if not ignoreNextRun is set (Performance issues)  
if (!ignoreNextRun) {  
Run (); 
}else {  
ignoreNextRun = false; 
return; 
}  
}  
}  
  
mainLoop = mainLoop &lt; 9999 ? mainLoop + 1 : 0; 
