void InitOnFirstStart () {
 Clear ();
 apps = new List &lt;SIOSAppData&gt; ();
 allBlocks = new List &lt;IMyTerminalBlock&gt; ();
 List &lt;IMyTerminalBlock&gt; _blocks = new List &lt;IMyTerminalBlock&gt; ();
 GridTerminalSystem.GetBlocksOfType &lt;IMyTimerBlock&gt; (_blocks);
 for (int i = 0; i &lt; _blocks.Count; i++) {
  if (_blocks [i].CustomName.Contains (FID_UPDATE)) {
   Update = (IMyTimerBlock) _blocks [i];
   autoUpdate = true;
  }
 }
 state ++;
}

void LoadExternalConfig () {
 int _requiredDataCnt = 0;
 string [] _data = Config.GetPublicText ().Split (CONFIG_SEPERATOR);
 for (int i = 0; i &lt; _data.Length; i++) {
  if (_data [i].Contains (CONFIG_KEY_PLATFORM_ID)) {
   platformID = _data [i].Replace (CONFIG_KEY_PLATFORM_ID, "").Trim ();
   _requiredDataCnt++;
  }
  else if (_data [i].Contains (CONFIG_KEY_CONDITION)) {
   for (int a = 0; a &lt; conditions.Length; a++) {
    if (_data [i].Contains (conditions [a].Title) &amp;&amp; currentCondition != a) {
     currentCondition = a;
     _requiredDataCnt++;
     break;
    }
   }
  }
  else if (_data [i].Contains (CONFIG_KEY_SECURITY)) {
   if (_data [i].ToLower ().Contains ("true"))
    securityEnabled = true;
   else
    securityEnabled = false;
   _requiredDataCnt++;
  }
  else if (_data [i].Contains (CONFIG_KEY_VERSION)) {
   if (_data [i].ToLower ().Contains ("true"))
    hideVersionInfoOnScreen = true;
   else
    hideVersionInfoOnScreen = false;
   _requiredDataCnt++;
  }
 }
 if (_data.Length &lt;= 1 || _requiredDataCnt &lt; 4) 
  StoreExternalConfig ();
}

void LockDown () {
 for (int i = 0; i &lt; doors.Count; i++) {
  if (doors [i] != null) {
   doors [i].ApplyAction ("OnOff_On");
   doors [i].ApplyAction ("Open_Off");
  }
  else {
   Echo ("Lost a door somewhere. Rebooting now.");
   state = 1;
   break;
  }
 }
}

void LogOn (string _id) {
 SIOSAppData _app = GetAppByID (_id);
 if (_app.ID == "Unknown")
  apps.Add (new SIOSAppData (_id));
 StoreExternalConfig ();
}

void LogOff (string _id) {
 SIOSAppData _app = GetAppByID (_id);
 if (_app.ID != "Unknown") {
  apps.RemoveAt (apps.IndexOf (_app));
 }
 else {
  Debug ("Error: could not unlogg App " + _id + ". App not found.");
 }
 StoreExternalConfig ();
}

void ManageCondition () {
 if (currentCondition &gt; 0 &amp;&amp; currentCondition &lt; conditions.Length) {
  if (conditionDecreaseTimer &lt;= 0) {
   currentCondition --;
   conditionDecreaseTimer = conditionDecreaseInterval;
   if (currentCondition &lt; conditions.Length - 2) 
    DisableSecurityBlocks (); 
  }
  else
   conditionDecreaseTimer --;
 }
}

bool ProcessArgument (string _arg) {
 switch (_arg) {
 case API_CLEAR: Clear (); return true;
 case API_DECREASE_CONDITION_LVL: 
  if (currentCondition &gt; 0) {
   currentCondition --;
   if (currentCondition &lt; conditions.Length - 1)
    DisableSecurityBlocks ();
   StoreExternalConfig ();
  }
  return true;
 case API_INCLUDE_JUMPDRIVE:
  if(includeJumpdrivesInPowerManagement)
   includeJumpdrivesInPowerManagement = false;
  else
   includeJumpdrivesInPowerManagement = true;
  Uninstall (platformID);
  state = 4;
  return true;
 case API_INCLUDE_GYRO:
  if(includeGyrosInPowerManagement)
   includeGyrosInPowerManagement = false;
  else
   includeGyrosInPowerManagement = true;
  Uninstall (platformID);
  state = 4;
  return true;
 case API_INCLUDE_THRUST:
  if(includeThrustersInPowerManagement)
   includeThrustersInPowerManagement = false;
  else
   includeThrustersInPowerManagement = true;
  Uninstall (platformID);
  state = 4;
  return true; 
case API_INCREASE_CONDITION_LVL: 
  if (currentCondition &lt; conditions.Length) {
   currentCondition ++;
   if (currentCondition &gt; conditions.Length - 1)
    EnableSecurityBlocks ();
   StoreExternalConfig ();
  }
  return true;
 case API_INSTALL: state = 4; return true;
 case API_HELP: return true;
 case API_HIDE_VERSION: hideVersionInfoOnScreen = true; StoreExternalConfig (); return true;
 case API_LOCKDOWN: LockDown (); return true;
 case API_POWER_UP: PowerUp (); return true;
 case API_REBOOT: state = 1; return true;
 case API_SECURITY_DISABLED: securityEnabled = false; StoreExternalConfig (); return true;
 case API_SECURITY_ENABLED: securityEnabled = true; StoreExternalConfig (); return true;
 case API_SET_CONDITION_LVL_MAX:
  if (currentCondition != conditions.Length) {
   currentCondition = conditions.Length;
   EnableSecurityBlocks ();
   StoreExternalConfig ();
  }
  return true;
 case API_SHUTDOWN: PowerDown (); return true;
 case API_SHOW_VERSION: hideVersionInfoOnScreen = false; StoreExternalConfig (); return true;
 case API_TRIM: Trim (); return true;
 case API_UNINSTALL: Uninstall (platformID); return true;
 case API_UNLOCK: Unlock (); return true;
 default: break;
 }
 if (_arg.StartsWith (API_APP_STATE_UPDATE)) {
  _arg = _arg.Replace (API_APP_STATE_UPDATE, "").Trim ();
  if (_arg.Contains (ORDER_SEPERATOR.ToString ())) {
   string [] _data = _arg.Split (ORDER_SEPERATOR);
   AppStateUpdate (_data [0].Trim (), _data [1].Trim ());
   state = 5;
   return true;
  }
 }
 else if (_arg.StartsWith (API_ADD_APP_CONFIG)) {
  _arg = _arg.Replace (API_ADD_APP_CONFIG, "").Trim ();
  string [] _data = _arg.Split (ORDER_SEPERATOR);
  SIOSAppData _app = GetAppByID (_data [0].Trim ());
  if (_app.ID != "Unknown") {
   _app.AddConfig (_data [1]);
  }
  else
   Echo ("App " + _data [0] + " could not be found.");
 }
 else if (_arg.StartsWith (API_INSTALL)) {
  _arg = _arg.Replace (API_INSTALL, "").Trim ();
  Install (_arg);
  return true;
 }
 else if (_arg.StartsWith (API_DELETE)) {
  _arg = _arg.Replace (API_DELETE, "").Trim ();
  Delete (_arg.Trim ());
  return true;
 }
 else if (_arg.StartsWith (API_LOGON)) {
  _arg = _arg.Replace (API_LOGON, "").Trim ();
  LogOn (_arg);
  return true;
 }
 else if (_arg.StartsWith (API_LOGOFF)) {
  _arg = _arg.Replace (API_LOGOFF, "").Trim ();
  LogOff (_arg);
  return true;
 }
 else if (_arg.StartsWith (API_REPLACE)) {
  _arg = _arg.Replace (API_REPLACE, "").Trim ();
  string [] _data = _arg.Split (ORDER_SEPERATOR);
  Replace (_data [0].Trim (), _data [1].Trim ());
  return true;
 }
 else if (_arg.StartsWith (API_UNINSTALL)) {
  _arg = _arg.Replace (API_UNINSTALL, "").Trim ();
  Uninstall (_arg);
  return true;
 }
 return false;
}

void PowerDown () {
 for (int i = 0; i &lt; poweredBlocks.Count; i++) {
  if (poweredBlocks [i] != null) {
   poweredBlocks [i].ApplyAction ("OnOff_Off");
  }
  else {
   Echo ("Lost a powered block somewhere. Rebooting now.");
   state = 1;
   break;
  }
 }
}

void PowerUp () {
 for (int i = 0; i &lt; poweredBlocks.Count; i++) {
  if (poweredBlocks [i] != null) {
   poweredBlocks [i].ApplyAction ("OnOff_On");
  }
  else {
   Echo ("Lost a powered block somewhere. Rebooting now.");
   state = 1;
   break;
  }
 }
}

string ReadConsoles () {
 string _info = "";
 for (int i = 0; i &lt; consoles.Count; i++) {
  if (consoles [i] != null) {
   if (_info.Length &lt; consoles [i].GetPublicText ().Length) {
    _info = consoles [i].GetPublicText ();
   }
  }
  else {
   Echo ("Lost a console somewhere. Rebooting now.");
   state = 1;
  }
 }
 return _info;
}

void Replace (string _toReplace, string _with) {
 for (int i = 0; i &lt; allBlocks.Count; i++) {
  if (allBlocks [i] != null) {
   string _name = allBlocks [i].CustomName;
   allBlocks [i].SetCustomName (_name.Replace (_toReplace, _with).Trim ());
  }
  else {
   Echo ("Lost a block somewhere. Rebooting now.");
   state = 1;
   break;
  }
 }
}

