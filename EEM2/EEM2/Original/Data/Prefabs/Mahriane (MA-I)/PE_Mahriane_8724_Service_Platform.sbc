int _min = hoveredListIndex - 2 &lt; 1 ? 1 : hoveredListIndex - 2; 
int _max = _min + 5 &gt; activeProductList.Count ? activeProductList.Count : _min + 5; 
  
for (int i = _min;i &lt; _max;i++) {  
  
