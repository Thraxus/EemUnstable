// GENERAL PROCESSING  
  
int run = 0; 
  
// State machine  
  
void Run () {  
switch (run) {  
case 0:  
LoadItems (); 
break; 
case 1:  
UpdateProductAmounts (); 
break; 
case 2:  
LoadValidItems (); 
break; 
case 3:  
if (Core != null) {  
if (Core.IsWorking) {  
StateUpdate (); 
}  
}  
break; 
case 4:  
LoadItems (); 
break; 
case 5:  
UpdateProductAmounts (); 
break; 
case 6:  
if (Core != null) {  
if (Core.IsWorking) {  
StateUpdate (); 
}  
}  
break; 
case 7:  
LoadValidItems (); 
break; 
case 8:  
EmptyCollectors (); 
break; 
case 9:  
if (useExternalConfig)  
LoadExternalConfigData (); 
break; 
default:  
if (Core != null) {  
if (Core.IsWorking) {  
StateUpdate (); 
}  
}  
run = -1; 
break; 
}  
  
// Check if platform has enough credit left to buy from customer  
  
if (currencyProduct != null) {  
if (currencyProduct.CurrentAmount &lt; MIN_CURRENCY_AMOUNT &amp;&amp; !onlyOffers) {  
onlyOffers = true; 
LoadValidItems (); 
}else {  
onlyOffers = false; 
}  
}else {  
onlyOffers = true; 
}  
  
// Check if tradeport has been enabled  
  
if (tradeEnabled) {  
  
// Empty the Collectors and the Deposit Container  
EmptyCollectors (); 
EmptyDeposit (); 
  
// Transfer left products from last run  
  
if (waitingProducts.Count &gt; 0)  
TransferWaitingProducts (); 
}  
  
run ++; 
}  
  
