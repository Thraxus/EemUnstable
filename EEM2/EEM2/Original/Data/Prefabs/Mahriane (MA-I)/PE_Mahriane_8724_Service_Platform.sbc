// It has to be called before the Run () method to update screen on Button Input  
// even if the run has to be ignored! NOTE: Input ignores the next Run ()!  
DisplayOnScreen (); 
  
// Update Script Info if not ignoreNextRun is set (Performance issues)  
if (!ignoreNextRun) {  
Run (); 
}else {  
ignoreNextRun = false; 
return; 
}  
}  
}  
  
mainLoop = mainLoop &lt; 9999 ? mainLoop + 1 : 0; 
