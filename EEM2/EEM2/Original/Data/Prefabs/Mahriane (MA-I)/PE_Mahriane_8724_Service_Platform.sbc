void Boot () {  
if (currentState &lt; 2)   
BootLists (); 
else if (currentState &lt; 3)   
BootBlocks (); 
else   
LoadItems (); 
currentState++; 
}  
  
void BootLists () {  
Echo ("Booting List"); 
portBlocks = new List &lt;IMyTerminalBlock&gt; (); 
storage = new List&lt;IMyInventory&gt; (); 
production = new List&lt;IMyInventory&gt; (); 
allItems = new List&lt;IMyInventoryItem&gt; (); 
validAquires = new List&lt;SIOSProduct&gt; (); 
validOffers = new List&lt;SIOSProduct&gt; (); 
activeProductList = new List&lt;SIOSProduct&gt; (); 
waitingProducts = new List&lt;SIOSProduct&gt; (); 
waitingProductsAmount = new List&lt;double&gt; (); 
collectors = new List&lt;IMyInventory&gt; (); 
}  
  
void BootBlocks () {  
Echo ("Booting Blocks"); 
int _requiredBlocks = 2; 
  
List &lt;IMyTerminalBlock&gt; _blocks = new List&lt;IMyTerminalBlock&gt; (); 
GridTerminalSystem.SearchBlocksOfName (platformID, _blocks); 
for (int i = 0;i &lt; _blocks.Count;i++) {  
string _name = _blocks [i].CustomName; 
if (_name.Contains (TRADE_PORT_ID)) {  
  
// GET OUTPUT CONTAINER //   
  
if (_name.Contains (FID_STORAGE))   
_blocks [i].CustomName = _name.Replace (FID_STORAGE, "Priv"); 
  
if (_name.Contains (FID_OUTPUT)) {  
 string _type = GetInventoryType (_name); 
if (_type == BID_CONTAINER) { 
 Output = ((IMyCargoContainer)_blocks [i]).GetInventory (0); 
} else if (_type == BID_CONNECTOR) { 
 Output = ((IMyShipConnector)_blocks [i]).GetInventory (0); 
} else { 
 Debug (_name + " is of wrong block type. Cargo Container or Connector required!", true); 
} 
_requiredBlocks--; 
}  
 
// GET DEPOSIT CONTAINER //  
 
else if (_name.Contains (FID_DEPOSIT)) { 
 string _type = GetInventoryType (_name); 
if (_type == BID_CONTAINER) { 
Deposit = ((IMyCargoContainer)_blocks [i]).GetInventory (0); 
} else { 
 Debug (_name + " is of wrong block type. Has to be a Cargo Container!", true); 
} 
_requiredBlocks--; 
}  
  
// GET PORT BLOCKs //   
  
if (_name.Contains (BID_COLLECTOR) || _name.Contains (BID_CONNECTOR) ||_name.Contains (FID_ACTIVE) ||_name.Contains (FID_INACTIVE)) {  
if (_name.Contains (FID_INACTIVE)) {  
_blocks [i].ApplyAction ("OnOff_On"); 
}else { 
_blocks [i].ApplyAction ("OnOff_Off"); 
} 
if (_name.Contains (BID_CONNECTOR)) { 
IMyShipConnector _con = (IMyShipConnector)_blocks [i]; 
// if (!_con.CollectAll) { 
// _con.ApplyAction ("CollectAll"); 
// } 
if (!_con.ThrowOut) { 
_con.ApplyAction ("ThrowOut"); 
} 
} 
 
portBlocks.Add (_blocks [i]); 
Debug ("Added " + _name + " to portBlocks.", true); 
if (_name.Contains (BID_COLLECTOR)) { 
collectors.Add (((IMyCollector) _blocks [i]).GetInventory (0)); 
} 
} 
 
// GET SORTER // 
 
else if (_name.Contains (BID_CONVEYOR_SORTER)) { 
_blocks [i].ApplyAction ("OnOff_On"); 
} 
}  
  
// GET STORAGE &amp; PRODUCTION INVENTORIEs //   
  
else if (_name.Contains (FID_STORAGE)) { 
 string _type = GetInventoryType (_name); 
 if(_type == BID_CONTAINER) { 
 storage.Add (((IMyCargoContainer) _blocks [i]).GetInventory (0)); 
 } else if(_type == BID_CONNECTOR) { 
 storage.Add (((IMyShipConnector) _blocks [i]).GetInventory (0)); 
 } else if(_type == BID_COLLECTOR) { 
 storage.Add (((IMyCollector) _blocks [i]).GetInventory (0)); 
 } else if(_type == BID_CONVEYOR_SORTER) {  
 storage.Add (((IMyConveyorSorter) _blocks [i]).GetInventory (0)); 
 } 
}else if (_name.Contains (FID_PRODUCTION)) { 
 string _type = GetInventoryType (_name); 
 if(_type == BID_ASSEMBLER) { 
  production.Add (((IMyAssembler) _blocks [i]).GetInventory (0)); 
 } else if(_type == BID_REFINERY) { 
  production.Add (((IMyRefinery) _blocks [i]).GetInventory (0)); 
 } else if(_type == BID_REACTOR) { 
  production.Add (((IMyReactor) _blocks [i]).GetInventory (0)); 
 } else if(_type == BID_CONTAINER) { 
  production.Add (((IMyCargoContainer) _blocks [i]).GetInventory (0)); 
 } else if(_type == BID_CONNECTOR) { 
  production.Add (((IMyShipConnector) _blocks [i]).GetInventory (0)); 
 } else if(_type == BID_COLLECTOR) { 
  production.Add (((IMyCollector) _blocks [i]).GetInventory (0)); 
 } else if(_type == BID_CONVEYOR_SORTER) { 
  production.Add (((IMyConveyorSorter) _blocks [i]).GetInventory (0)); 
 } 
}  
}  
  
// ENABLE SCRIPT IF ALL REQUIRED CHECKs SUCCEED   
if (_requiredBlocks &lt;= 0 &amp;&amp; TradeDisplay != null &amp;&amp; storage.Count &gt; 0) { 
scriptEnabled = true; 
Debug (FID_STORAGE + ": " + storage.Count.ToString (), true); 
Debug (FID_PRODUCTION + ": " + production.Count.ToString (), true); 
}  
else {  
if (TradeDisplay == null)  
Debug ("&gt; CRITICAL: No Trade Display found.", true); 
if (Deposit == null)  
Debug ("&gt; CRITICAL: Deposit not found.", true); 
if (Output == null)  
Debug ("&gt; CRITICAL: Output not found.", true); 
if (storage.Count &lt;= 0)  
Debug ("&gt; CRITICAL: No Storage Space found.", true); 
}  
}  
  
void LoadExternalConfigData () {  
Echo ("Loading External Config Data"); 
if (Config != null) {  
string[] _data = Config.GetPublicText ().Split (CONFIG_SEPERATOR); 
for (int i = 0;i &lt; _data.Length;i++) {  
if (_data [i].Contains (CONFIG_KEY_PLATFORM_ID)) {  
platformID = (_data [i].Replace (CONFIG_KEY_PLATFORM_ID, "")).Trim (); 
}  
else if (_data [i].Contains (CONFIG_KEY_VERSION)) {  
if (_data [i].ToLower ().Contains ("true"))   
hideVersionInfoOnScreen = true; 
else   
hideVersionInfoOnScreen = false; 
}  
}  
}else {  
useExternalConfig = false; 
Debug ("Could not load ext. config data. File not found!", true); 
}  
}  
  
void LoadItems () {  
Echo ("Loading Items"); 
// Get All Items in storage   
allItems.Clear (); 
  
for (int i = 0;i &lt; storage.Count;i++) {  
if (storage [i] != null)   
allItems.AddRange (storage [i].GetItems ()); 
}  
  
if (currentState &lt; 3) {  
// Update all Product Amounts   
UpdateProductAmounts (); 
// Load valid Items   
LoadValidItems (); 
currentState++; 
}  
}  
  
void UpdateProductAmounts () {  
Echo ("Updateing Product Amounts"); 
for (int i = 0;i &lt; products.Count;i++) {  
if (products [i].SubTypeID.Trim () != "") {  
products [i].CurrentAmount = GetItemAmountInStorage (products [i].SubTypeID, products [i].ItemClass); 
if (products [i].SubTypeID == CURRENCY_ID &amp;&amp; products [i].ItemClass == CURRENCY_CLASS) {  
currencyProduct = products [i]; 
}  
}  
}  
}  
  
void LoadValidItems () {  
Echo ("Loading Valid Items"); 
validAquires.Clear (); 
validOffers.Clear (); 
  
List &lt;bool&gt; _aquires = roles [platformRoleID].Aquires; 
List &lt;bool&gt; _offers = roles [platformRoleID].Offers; 
List &lt;string&gt; _itemClasses = new List &lt;string&gt; (); 
  
_itemClasses.AddRange (itemClasses); 
  
for (int i = 1;i &lt; products.Count;i++) {  
if (products [i].SubTypeID == "Ice") {  
if (_aquires [0] &amp;&amp; !onlyOffers)   
validAquires.Add (products [i]); 
if (_offers [0] &amp;&amp; products [i].CurrentAmount &gt; 0)   
validOffers.Add (products [i]); 
}else if (products [i].SubTypeID == CURRENCY_ID &amp;&amp; products [i].ItemClass == CURRENCY_CLASS) {  
validAquires.Add (products [i]); 
}else if (products [i].ItemClass.Contains (_itemClasses [4])) {  
if (_aquires [4] &amp;&amp; !onlyOffers)   
validAquires.Add (products [i]); 
if (_offers [4] &amp;&amp; products [i].CurrentAmount &gt; 0)   
validOffers.Add (products [i]); 
}else {  
if (_aquires [_itemClasses.IndexOf (products [i].ItemClass)] &amp;&amp; !onlyOffers) {  
validAquires.Add (products [i]); 
}  
if (_offers [_itemClasses.IndexOf (products [i].ItemClass)] &amp;&amp; products [i].CurrentAmount &gt; 0) {  
validOffers.Add (products [i]); 
}  
}  
}  
Debug ("Added " + validAquires.Count.ToString () + " valid Aquires.", true); 
Debug ("Added " + validOffers.Count.ToString () + " valid Offers.", true); 
}  
  
double GetItemAmountInStorage (string _subtypeID, string _itemClass) {  
double _amount = 0; 
for (int i = 0;i &lt; allItems.Count;i++) {  
if (allItems [i] != null) {  
if (allItems [i].Content.SubtypeName == _subtypeID &amp;&amp; allItems [i].Content.ToString ().Contains (_itemClass))   
_amount += (double) allItems [i].Amount; 
}  
}  
return _amount; 
}  
  
