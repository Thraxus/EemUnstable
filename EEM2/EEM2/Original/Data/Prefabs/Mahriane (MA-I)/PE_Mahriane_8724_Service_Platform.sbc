void EnableSecurityBlocks () {
 for (int i = 0; i &lt; securityBlocks.Count; i++) {
  if (securityBlocks [i] != null) {
   string _id = securityBlocks [i].CustomName;
   securityBlocks [i].ApplyAction ("OnOff_On");
   if (_id.Contains (BID_GRAVITY)) {
    securityBlocks [i].ApplyAction ("OnOff_Off");
   }
   else if (_id.Contains (BID_GRAVITY_SPHERE)) {
    securityBlocks [i].ApplyAction ("OnOff_Off");
   }
   else if (_id.Contains (BID_DOOR)) {
    IMyDoor _door = (IMyDoor) securityBlocks [i];
    _door.ApplyAction ("Open_Off");
   }
   else if (_id.Contains (BID_SOUNDBLOCK)) {
    IMySoundBlock _speaker = (IMySoundBlock) securityBlocks [i];
    _speaker.ApplyAction ("Play");
   }
   else if (_id.Contains (BID_TEXTPANEL)) {
    MyExtendedTextPanel _panel = GetPanelByInstance ((IMyTextPanel) securityBlocks [i]);
    _panel.Instance.SetValue ("BackgroundColor", conditions [currentCondition].BackgroundColor);
    _panel.Instance.SetValue ("FontColor", conditions [currentCondition].FontColor);
   }
   else if (_id.Contains (BID_WARHEAD)) {
    IMyWarhead _warHead = (IMyWarhead) securityBlocks [i];
    _warHead.ApplyAction ("Start");
   }
  }
  else {
   Echo ("Lost a securityBlock somewhere. Rebooting now.");
   state = 1;
   break;
  }
 }
}

string GetAirVentID (IMyAirVent _vent) {
 string _id = _vent.CustomName;
 return _id.Substring (_id.IndexOf (HASH) + 1, 3);
}

SIOSAppData GetAppByID (string _id) {
 for (int i = 0; i &lt; apps.Count; i++) {
  if (apps [i].ID == _id)
   return apps [i];
 }
 return new SIOSAppData ("Unknown");
}

string GetOrderLine (string _consoleValue) {
 string _lastLine = "";
 string [] _data = _consoleValue.Split (LINE_SEPERATOR);
 _lastLine = _data [_data.Length - 1].Trim ();
 return _lastLine;
}

MyExtendedInteriorLight GetLightByInstance (IMyInteriorLight _inst) {
 for (int i = 0; i &lt; lights.Count; i++) {
  if (_inst == lights [i].Instance)
   return lights [i];
 }
 return new MyExtendedInteriorLight (_inst, _inst.GetValue &lt;Color&gt; ("Color"));
}

MyExtendedTextPanel GetPanelByInstance (IMyTextPanel _inst) {
 for (int i = 0; i &lt; panels.Count; i++) {
  if (_inst == panels [i].Instance)
   return panels [i];
 }
 return new MyExtendedTextPanel (_inst, _inst.GetValue &lt;Color&gt; ("BackgroundColor")
   , _inst.GetValue &lt;Color&gt; ("FontColor"));
}

