// GENERAL PROCESSING  
  
int run = 0; 
  
// State machine  
  
void Run () {  
switch (run) {  
case 0:  
LoadItems (); 
break; 
case 1:  
UpdateProductAmounts (); 
break; 
case 2:  
LoadValidItems (); 
break; 
case 3:  
if (Core != null) {  
if (Core.IsWorking) {  
StateUpdate (); 
}  
}  
break; 
case 4:  
LoadItems (); 
break; 
case 5:  
UpdateProductAmounts (); 
break; 
case 6:  
if (Core != null) {  
if (Core.IsWorking) {  
StateUpdate (); 
}  
}  
break; 
case 7:  
LoadValidItems (); 
break; 
case 8:  
EmptyCollectors (); 
break; 
case 9:  
if (useExternalConfig)  
LoadExternalConfigData (); 
break; 
default:  
if (Core != null) {  
if (Core.IsWorking) {  
StateUpdate (); 
}  
}  
run = -1; 
break; 
}  
  
// Check if platform has enough credit left to buy from customer  
  
if (currencyProduct != null) {  
if (currencyProduct.CurrentAmount &lt; MIN_CURRENCY_AMOUNT &amp;&amp; !onlyOffers) {  
onlyOffers = true; 
LoadValidItems (); 
}else {  
onlyOffers = false; 
}  
}else {  
onlyOffers = true; 
}  
  
// Check if tradeport has been enabled  
  
if (tradeEnabled) {  
  
// Empty the Collectors and the Deposit Container  
EmptyCollectors (); 
EmptyDeposit (); 
  
// Transfer left products from last run  
  
if (waitingProducts.Count &gt; 0)  
TransferWaitingProducts (); 
}  
  
run ++; 
}  
  
bool ProcessArgument (string _arg) {  
switch (_arg) {  
case API_CLEAR:  
Clear (); 
return true; 
case API_REBOOT:  
if (tradeEnabled) {  
AbortTrade (); 
}  
currentState = 1; 
ignoreNextRun = true; 
return true; 
case API_INPUT_CANCEL:  
ignoreNextRun = true; 
Cancel (); 
return true; 
case API_INPUT_CONFIRM:  
ignoreNextRun = true; 
Confirm (); 
return true; 
case API_INPUT_LEFT:  
ignoreNextRun = true; 
Left (); 
return true; 
case API_INPUT_RIGHT:  
ignoreNextRun = true; 
Right (); 
return true; 
case API_TRADE_ENABLED:  
EnablePortBlocks (); 
tradeEnabled = true; 
return true; 
case API_TRADE_DISABLED:  
DisablePortBlocks (); 
tradeEnabled = false; 
return true; 
default:  
break; 
}  
return false; 
}  
  
// TRADE PROCESSING  
  
void DisablePortBlocks () {  
for (int i = 0;i &lt; portBlocks.Count;i++) {  
if (portBlocks [i] != null) {  
if (portBlocks [i].CustomName.Contains (FID_INACTIVE)) {  
portBlocks [i].ApplyAction ("OnOff_On"); 
}else {  
portBlocks [i].ApplyAction ("OnOff_Off"); 
}  
}else {  
currentState = 1; 
}  
}  
}  
  
void EnablePortBlocks () {  
for (int i = 0;i &lt; portBlocks.Count;i++) { 
if (portBlocks [i] != null) { 
if (portBlocks [i].CustomName.Contains (FID_INACTIVE)) { 
portBlocks [i].ApplyAction ("OnOff_Off"); 
}else { 
portBlocks [i].ApplyAction ("OnOff_On"); 
} 
}else { 
currentState = 1; 
} 
} 
} 
 
