private class SIOSProduct {  
  
bool stackable; 
string subtypeID; 
string itemClass; 
string unit; 
double baseValue; 
double highAmount; 
double lowAmount; 
double amount; 
double volume; 
double MARGIN; 
double minPricePercent; 
int minPackageSize; 
  
public List &lt;string&gt; ItemNames; 
  
public bool Stackable {  
get {  
return stackable; 
}  
}  
  
public string SubTypeID {  
get {  
return subtypeID; 
}  
}  
  
public string ItemClass {  
get {  
return itemClass; 
}  
}  
  
public string Unit {  
get {  
return unit; 
}  
}  
  
public string PriceIndicator {  
get {  
if (amount &lt; lowAmount)  
return "[^]"; 
else if (amount &gt; highAmount)  
return "[v]"; 
else  
return "[]"; 
}  
}  
  
public double CurrentAmount {  
get {  
return amount; 
}  
set {  
if (value &gt; 0)  
amount = value; 
else  
amount = 0; 
}  
}  
  
public double OfferValue {  
get {  
return AquireValue + (AquireValue * (MARGIN / 100)); 
}  
}  
  
public double AquireValue {  
get {  
double _val = amount &gt; lowAmount ? baseValue : baseValue * (1 - (amountPercent / 100)); 
_val = _val &gt; (baseValue * (minPricePercent / 100)) ? _val : (baseValue * (minPricePercent / 100)); 
return _val; 
}  
}  
  
public double Volume {  
get {  
return volume; 
}  
}  
  
public double PricePercentage (string _mode) {  
return _mode == "Offer" ? (100 / baseValue * OfferValue) - MARGIN : (100 / baseValue * AquireValue); 
}  
  
private double amountPercent {  
get {  
return 100 / highAmount * amount; 
}  
}  
  
public int MinPackageSize {  
get {  
return minPackageSize; 
}  
}  
  
public SIOSProduct (string _id, string _class, bool _stack, string _unit, double _bValue, double _resFac,  
double _minPricePercent, double _highAmount, double _MARGIN, double _vol, int _minPackage, string [] _names) {  
  
amount = 0; 
stackable = _stack; 
subtypeID = _id; 
itemClass = _class; 
unit = _unit; 
baseValue = _bValue * _resFac; 
minPricePercent = _minPricePercent; 
lowAmount = _highAmount * 0.2; 
highAmount = _highAmount; 
volume = _vol; 
MARGIN = _MARGIN; 
minPackageSize = _minPackage; 
  
ItemNames = new List&lt;string&gt; (); 
ItemNames.AddArray (_names); 
  
int _maxLength = 50; 
  
for (int i = 0;i &lt; ItemNames.Count;i++) {  
ItemNames [i] += GetPlaceHolders (ItemNames [i], _maxLength); 
}  
}  
  
string GetPlaceHolders (string _input, int _length) {  
int _amount = CheckChar (_input.ToCharArray ()); 
  
_amount = _length - _amount; 
  
string _info = ""; 
  
for (int i = 0;i &lt; _amount;i++) {  
_info += "."; 
}  
  
return _info; 
}  
  
int CheckChar (char [] _chars) {  
int _amount = 0; 
  
for (int i = 0;i &lt; _chars.Length;i++) {  
switch (_chars [i]) {  
case '1':  
_amount += 1; 
break; 
case 'l':  
_amount += 1; 
break; 
case 'i':  
_amount += 1; 
break; 
case 'I':  
_amount += 1; 
break; 
case 'f':  
_amount += 1; 
break; 
case 't':  
_amount += 1; 
break; 
case 'r':  
_amount += 1; 
break; 
case 'M':  
_amount += 3; 
break; 
case 'm':  
_amount += 2; 
break; 
case 'w':  
_amount += 2; 
break; 
case 'W':  
_amount += 3; 
break; 
case '.':  
_amount += 1; 
break; 
case '(':  
_amount += 1; 
break; 
case ')':  
_amount += 1; 
break; 
case '[':  
_amount += 1; 
break; 
case ']':  
_amount += 1; 
break; 
default:  
_amount += 2; 
break; 
}  
}  
return _amount; 
}  
  
