ignoreNextRun = true; 
return true; 
case API_INPUT_CANCEL:  
ignoreNextRun = true; 
Cancel (); 
return true; 
case API_INPUT_CONFIRM:  
ignoreNextRun = true; 
Confirm (); 
return true; 
case API_INPUT_LEFT:  
ignoreNextRun = true; 
Left (); 
return true; 
