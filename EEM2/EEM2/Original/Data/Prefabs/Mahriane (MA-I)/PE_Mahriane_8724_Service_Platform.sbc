string DoProductListScreen (string _mode) {  
mode = _mode; 
string _info = ""; 
  
// &lt;&lt; HEADER &gt;&gt; //   
  
UpdateActiveProductList (); 
  
if (_mode == "Offer") {  
_info = TAB + menuText [16] + BLANK; 
}else {  
_info = TAB + menuText [17] + BLANK; 
}  
  
_info += BLANK; 
  
// Only show procut list if Offer mode is active or there is enough currency to buy stuff from customer   
  
if (!onlyOffers || _mode == "Offer") {  
  
hoveredListIndex = hoveredListIndex &gt; activeProductList.Count ? activeProductList.Count - 1 : hoveredListIndex; 
selectedListIndex = selectedListIndex &gt; activeProductList.Count ? 0 : selectedListIndex; 
  
if (activeProductList.Count &gt; 0) {  
  
activeProductList.Insert (0, products [0]); 
selectionRange = activeProductList.Count; 
  
// &lt;&lt; BODY &gt;&gt; //   
  
int _min = hoveredListIndex - 2 &lt; 1 ? 1 : hoveredListIndex - 2; 
int _max = _min + 5 &gt; activeProductList.Count ? activeProductList.Count : _min + 5; 
  
for (int i = _min;i &lt; _max;i++) {  
  
// Get Corret Pre and Suffix for line   
  
hoveredListIndex = hoveredListIndex &gt; _max ? _max - 1 : hoveredListIndex; 
  
if (activeProductList [hoveredListIndex].ItemClass == itemClasses [6]) {  
packageSize = 1; 
}  
  
string _prefix = ""; 
string _suffix = ""; 
  
if (i == selectedListIndex) {  
_prefix = "&lt; "; 
_suffix = " &gt;"; 
}else if (i == hoveredListIndex) {  
hoveredProductIndex = products.IndexOf (activeProductList [hoveredListIndex]); 
_prefix = "[ "; 
_suffix = " ]"; 
}  
  
// Get Correct Price (Offer or Aquire)   
  
double _price = _mode == "Offer" ? activeProductList [i].OfferValue : activeProductList [i].AquireValue; 
string _priceStrg = _price &gt;= 0.01 ? _price.ToString ("00.00") : "&lt; 0.01"; 
  
_info += TAB + _prefix + activeProductList [i].ItemNames [languageID] + _priceStrg + CURRENCY_UNIT + _suffix + BLANK; 
  
}  
  
// Create empty spaces at the end of the list to keep page format   
  
if (hoveredListIndex == activeProductList.Count - 2) {  
_info += BLANK; 
}else if (hoveredListIndex == activeProductList.Count - 1) {  
_info += BLANK + BLANK; 
}  
  
_info += BLANK; 
  
// &lt;&lt; FOOTER &gt;&gt; //   
if (hoveredListIndex &gt; 0) {  
_info += GetDetailedProductInfo (activeProductList [hoveredListIndex]); 
}else {  
_info += BLANK + BLANK + BLANK + BLANK; 
}  
}  
}else {  
  
// There isn't enough currency aboard to buy from customer so excuse please   
  
_info += BLANK; 
_info += TAB + "We are sorry, but we only Accept " + currencyProduct.ItemNames [languageID].Replace (".","") + "." + BLANK; 
_info += BLANK; 
_info += BLANK; 
_info += BLANK; 
_info += BLANK; 
_info += BLANK; 
_info += BLANK; 
_info += BLANK; 
_info += BLANK; 
_info += BLANK; 
_info += BLANK; 
}  
return _info; 
}  
  
