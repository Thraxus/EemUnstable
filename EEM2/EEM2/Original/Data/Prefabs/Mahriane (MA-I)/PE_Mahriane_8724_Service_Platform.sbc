void Main (string argument) {
 Echo (TimeStamp + "[" + mainLoop.ToString ("000") + "]: " + platformID + EMPTY + stateNames [state]);
 Echo ("Ext. Config:   " + useExternalConfig.ToString ());
 Echo ("AutoUpdate: " + autoUpdate.ToString ());
 Echo ("Condition: " + conditions [currentCondition].Title);
 if (mainLoop &gt;= 999) {
  state = 1;
  mainLoop = 0;
 }
 if (state &lt;= 0)
  InitOnFirstStart ();
 else {
  if (state &lt; 3)
   Boot ();
  else {
   if (useExternalConfig) {
    if (Config != null) {
     LoadExternalConfig ();
    }
    else {
     useExternalConfig = false;
     Echo ("Config file not found. Rebooting now.");
     state = 1;
    }
   }
   if (argument.Trim () == "") {
    if (consoles.Count &gt; 0) {
     consoleInfo = ReadConsoles ();
     if (consoleInfo.Trim () != "")
      argument = GetOrderLine (consoleInfo);
    }
   }
   if (argument.Trim () != "") {
    if (!ProcessArgument (argument)) {
     Echo ("Recieved invalid argument: " + argument);
    }
   }
   if (state &lt; 5) {
    if (state &lt; 4) {
     if (securityEnabled) {
      CheckVents ();
      ManageCondition ();
     }
     else {
      currentCondition = 0;
     }
     Display ();
    }
    else
     Install (platformID);
   }
   else {
    state = 3;
    return;
   }
  }
 }

 if (autoUpdate) {
  if (Update != null) {
   Update.ApplyAction ("Start");
  }
  else {
   autoUpdate = false;
   Echo ("Update timer has been lost somewhere. Rebooting now.");
   state = 1;
  }
 }
 mainLoop ++;
 ResetLocalValues ();
}

void AppStateUpdate (string _appID, string _state) {
 SIOSAppData _app = GetAppByID (_appID);
 if (_app.ID != "Unknown") {
  _app.State = _state;
  StoreExternalConfig ();
 }
}

void Boot () {
 if (state &lt; 2) {
  BootLists ();
 }
 else if (state &lt; 3) {
  BootBlocks ();
 }
 state ++;
}

void BootBlocks () {
 GridTerminalSystem.GetBlocksOfType &lt;IMyTerminalBlock&gt; (allBlocks);

 for (int i = 0; i &lt; allBlocks.Count; i++) {
  if (allBlocks [i] != null) {
   string _id = allBlocks [i].CustomName;
   if (_id.Contains (platformID + BID_SEPERATOR)) {
    if (_id.Contains (BID_SEPERATOR + BID_TEXTPANEL)) {
     IMyTextPanel _panel = (IMyTextPanel) allBlocks [i];
     string _title = _panel.GetPublicTitle ();
     if (_title.Contains (FID_CONFIG) || _title == FID_CONFIG) {
      Config = _panel;
      Config.SetValue ("BackgroundColor", default_BG_Color);
      Config.SetValue ("FontColor", default_Font_Color);
      Config.SetValueFloat ("FontSize", default_Font_Size);
      StoreExternalConfig ();
      useExternalConfig = true;
     }
     else if (_title.Contains (FID_CONSOLE) || _title == FID_CONSOLE) {
      _panel.SetValue ("BackgroundColor", default_BG_Color);
      _panel.SetValue ("FontColor", default_Font_Color);
      _panel.SetValueFloat ("FontSize", default_Font_Size);
      _panel.WritePublicText (ConsoleInfo);
      _panel.ShowTextureOnScreen ();
      _panel.ShowPublicTextOnScreen ();
      consoles.Add (_panel);
     }
     else {
      panels.Add (new MyExtendedTextPanel (_panel, _panel.GetValue &lt;Color&gt; ("BackgroundColor")
        , _panel.GetValue &lt;Color&gt; ("FontColor")));
     }
    }
    else if (_id.Contains (BID_DOOR + BID_SEPERATOR)) {
     doors.Add ((IMyDoor) allBlocks [i]);
    }
    else if (_id.Contains (BID_LIGHT_INTERIOR + BID_SEPERATOR)) {
     lights.Add (new MyExtendedInteriorLight ((IMyInteriorLight) allBlocks [i],
      ((IMyInteriorLight) allBlocks [i]).GetValue &lt;Color&gt; ("Color")));
    }
    if (_id.Contains (FID_SECURITY)) {
     securityBlocks.Add (allBlocks [i]);
    }
    if (_id.Contains (FID_POWERED)) {
     poweredBlocks.Add (allBlocks [i]);
    }
   }
   else {
    allBlocks.RemoveAt (i);
    i--;
   }
  }
 }
}

