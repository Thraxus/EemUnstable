void CheckVents () {
 for (int i = 0; i &lt; airVents.Count; i++) {
  if (airVents [i]!= null) {
   string _id = GetAirVentID (airVents [i]);
   if (!airVents [i].IsPressurized ()) {
    Debug (airVents [i].CustomName + " is not pressurized.");
    CloseDoors (_id);
    if (disableDoorsTimer &lt;= 0) {
     DisableDoors (_id);
    }
   }
   else {
    EnableDoors (_id);
   }
  }
  else {
   Echo ("Lost an airVent somewhere. Rebooting now.");
   state = 1;
   break;
  }
 }

 if (disableDoorsTimer &gt; 0)
  disableDoorsTimer--;
}

void Clear () {
 debugMessages = new List &lt;string&gt; ();
 Debug ("Debugger started.");
}

void CloseDoors (string _id) {
 for (int i = 0; i &lt; doors.Count; i++) {
  if (doors [i] != null) {
   doors [i].ApplyAction ("OnOff_On");
   doors [i].ApplyAction ("Open_Off");
   disableDoorsTimer = disableDoorsInterval;
  }
  else {
   Echo ("Lost a door somewhere. Rebooting now.");
   state = 1;
   break;
  }
 }
}

void Debug (string _msg) {
 debugMessages.Add (TimeStamp + ": " + _msg);
}

void Delete (string _toDelete) {
 Replace (_toDelete, "");
}

void DisableDoors (string _id) {
 for (int i = 0; i &lt; doors.Count; i++) {
  if (doors [i] != null) {
   doors [i].ApplyAction ("OnOff_Off");
  }
  else {
   Echo ("Lost a door somewhere. Rebooting now.");
   state = 1;
   break;
  }
 }
}

void DisableSecurityBlocks () {
 for (int i = 0; i &lt; securityBlocks.Count; i++) {
  if (securityBlocks [i] != null) {
   string _id = securityBlocks [i].CustomName;
   securityBlocks [i].ApplyAction ("OnOff_Off");
   if (_id.Contains (BID_GRAVITY)) {
    securityBlocks [i].ApplyAction ("OnOff_On");
   }
   else if (_id.Contains (BID_GRAVITY_SPHERE)) {
    securityBlocks [i].ApplyAction ("OnOff_On");
   }
   else if (_id.Contains (BID_DOOR)) {
    IMyDoor _door = (IMyDoor) securityBlocks [i];
    _door.ApplyAction ("Open_On");
   }
   else if (_id.Contains (BID_LIGHT_INTERIOR)) {
    MyExtendedInteriorLight _light = GetLightByInstance ((IMyInteriorLight) securityBlocks [i]);
    _light.Instance.SetValue ("Color", _light.DefaultColor);
   }
   else if (_id.Contains (BID_SOUNDBLOCK)) {
    IMySoundBlock _speaker = (IMySoundBlock) securityBlocks [i];
    _speaker.ApplyAction ("Stop");
   }
   else if (_id.Contains (BID_TEXTPANEL)) {
    MyExtendedTextPanel _panel = GetPanelByInstance ((IMyTextPanel) securityBlocks [i]);
    _panel.Instance.SetValue ("BackgroundColor", _panel.DefaultBackground);
    _panel.Instance.SetValue ("FontColor", _panel.DefaultFont); 
   }
   else if (_id.Contains (BID_WARHEAD)) {
    IMyWarhead _warHead = (IMyWarhead) securityBlocks [i];
    _warHead.ApplyAction ("Stop");
   }
  }
  else {
   Echo ("Lost a securityBlock somewhere. Rebooting now.");
   state = 1;
   break;
  }
 }
}

void Display () {
 for (int i = 0; i &lt; panels.Count; i++) {
  if (panels [i].Instance != null) {
   string _title = panels [i].Instance.GetPublicTitle ();
   if (_title == FID_DEBUG) {
    panels [i].Instance.ShowPublicTextOnScreen ();
    panels [i].Instance.SetValue ("BackgroundColor", new Color (0,0,255,255));
    panels [i].Instance.SetValue ("FontColor", new Color (255,255,0,255));
    panels [i].Instance.SetValueFloat ("FontSize", 0.6f);
    panels [i].Instance.WritePublicText (DebugInfo);
   }
   else if (_title.StartsWith ("$IOS")) {
    panels [i].Instance.WritePublicText (_title + BLANK + BLANK + DefaultInfo);
   }
  }
  else {
   Echo ("Lost a text panel somewhere. Rebooting now.");
   state = 1;
  }
 }
}

