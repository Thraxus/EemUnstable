if (debugMessages.Count &gt; debugMax) {  
Clear (); 
}  
  
debugMessages.Insert (0, "[" + TimeStamp + "]: " + _msg); 
  
if (_updateInstant) {  
if (Debugger != null) {  
Debugger.WritePublicText (GetDebugInfo); 
Debugger.ShowTextureOnScreen (); 
Debugger.ShowPublicTextOnScreen (); 
}  
}  
