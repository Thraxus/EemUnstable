 debugInfo = "";
}

void StoreExternalConfig () {
 Config.ShowPublicTextOnScreen ();
 string _config = 
 CONFIG_SEPERATOR + EMPTY + TimeStamp + "[" + mainLoop.ToString ("000") + "]" + EMPTY + Version + BLANK
 + CONFIG_SEPERATOR + EMPTY + CONFIG_KEY_PLATFORM_ID + platformID + BLANK
 + CONFIG_SEPERATOR + EMPTY + CONFIG_KEY_CONDITION + ": " + conditions [currentCondition].Title + BLANK
 + CONFIG_SEPERATOR + EMPTY + CONFIG_KEY_SECURITY + ": " + securityEnabled.ToString () + BLANK
 + CONFIG_SEPERATOR + EMPTY + CONFIG_KEY_VERSION + ": " + hideVersionInfoOnScreen.ToString () + BLANK
 + CONFIG_SEPERATOR + EMPTY + BLANK;
 _config += CONFIG_SEPERATOR + EMPTY + BLANK;
 _config += CONFIG_SEPERATOR + "&lt;&lt; Apps &gt;&gt;" + BLANK;
 _config += CONFIG_SEPERATOR + EMPTY + BLANK;

 if ( apps.Count &gt; 0) {
  for (int i = 0; i &lt; apps.Count; i++) {
   _config += CONFIG_SEPERATOR + EMPTY + apps [i].ID + ": " + apps [i].State + BLANK;
   for (int a = 0; a &lt; apps [i].Config.Count; a++) {
    _config += CONFIG_SEPERATOR + EMPTY + "-" + EMPTY  + apps [i].Config [a] + BLANK;
   }
  }
 }
 else {
  _config += CONFIG_SEPERATOR + EMPTY + " No Apps installed.";
 }
 Config.WritePublicText (_config);
 Config.ShowTextureOnScreen ();
 Config.ShowPublicTextOnScreen ();
}

void Trim () {
 for (int i = 0; i &lt; allBlocks.Count; i++) {
  if (allBlocks [i] != null) {
   allBlocks [i].SetCustomName (allBlocks [i].CustomName.Trim ());
  }
  else {
   Echo ("Lost a block somewhere. Rebooting now.");
   state = 1;
  }
 }
}

void Uninstall (string _pID) {
 for (int i = 0; i &lt; allBlocks.Count; i++) {
  if (allBlocks [i] != null) {
   string _oldID = allBlocks [i].CustomName;
   if (_oldID.Contains (_pID + BID_SEPERATOR) &amp;&amp; _oldID.Contains (BID_SUFFIX.ToString ())) {
    int _start = _oldID.IndexOf (BID_SUFFIX) + 1;
    int _length = _oldID.Length - _start;
    string _newID = _oldID.Substring (_start, _length);
    allBlocks [i].SetCustomName (_newID.Trim ());
   }
   else
    continue;
  }
  else {
   Echo ("Lost a block somewhere. Rebooting now.");
  }
 }
 state = 1;
}

void Unlock () {
 for (int i = 0; i &lt; doors.Count; i++) {
  if (doors [i] != null) {
   if (!doors [i].CustomName.Contains (FID_AIRLOCK)) {
    doors [i].ApplyAction ("OnOff_On");
    doors [i].ApplyAction ("Open_On");
   }
  }
  else {
   Echo ("Lost a door somewhere. Rebooting now.");
   state = 1;
   break;
  }
 }
}

