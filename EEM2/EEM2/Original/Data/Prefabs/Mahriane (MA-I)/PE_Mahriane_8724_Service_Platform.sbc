   consoleInfo = TimeStamp + "[" + mainLoop.ToString ("000") + "]: " + Version + BLANK + BLANK;
   consoleInfo += "$&gt;&gt; ";
  }
  return consoleInfo;
 }
}

string DebugInfo {
 get {
  if (debugInfo.Trim () == "") {
   debugInfo += TimeStamp + "[" + mainLoop.ToString ("000") + "]: " + Version + BLANK + BLANK;
   for (int i = 0; i &lt; debugMessages.Count; i++) {
    debugInfo += debugMessages [i] + BLANK;
   }
  }
  return debugInfo;
 }
}

