private class SIOSProduct {  
  
bool stackable; 
string subtypeID; 
string itemClass; 
string unit; 
double baseValue; 
double highAmount; 
double lowAmount; 
double amount; 
double volume; 
double MARGIN; 
double minPricePercent; 
int minPackageSize; 
  
public List &lt;string&gt; ItemNames; 
  
public bool Stackable {  
get {  
return stackable; 
}  
}  
  
public string SubTypeID {  
get {  
return subtypeID; 
}  
}  
  
public string ItemClass {  
get {  
return itemClass; 
}  
}  
  
public string Unit {  
get {  
return unit; 
}  
}  
  
public string PriceIndicator {  
get {  
if (amount &lt; lowAmount)  
return "[^]"; 
else if (amount &gt; highAmount)  
return "[v]"; 
else  
return "[]"; 
}  
}  
  
public double CurrentAmount {  
get {  
return amount; 
}  
set {  
if (value &gt; 0)  
